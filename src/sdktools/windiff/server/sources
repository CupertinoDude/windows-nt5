INCLUDES=..\..\gutils

UMENTRY=winmain

TARGETNAME=sumserve
TARGETPATH=obj
TARGETTYPE=PROGRAM

UMTYPE=console

C_DEFINES=-DWIN32

SOURCES=sumserve.c \
        queue.c    \
        scan.c     \
        files.c    \
        file.c     \
        service.c  \
        errlog.c   \
        debug.c

TARGETLIBS=$(PROJECT_ROOT)\windiff\gutils\$(O)\gutils.lib          \
        $(SDK_LIB_PATH)\mpr.lib \
        $(SDK_LIB_PATH)\wsock32.lib \
        $(SDK_LIB_PATH)\user32.lib
