C_DEFINES=-UNICODE

TARGETNAME=relstat
TARGETPATH=obj
TARGETTYPE=PROGRAM

INCLUDES=\
    $(O);\
    ..\; \
    ..\inc; \
    $(BASE_INC_PATH)

IDL_TYPE=RPC
PASS0_HEADERDIR=$(O)
PASS0_CLIENTDIR=$(O)
PASS0_SERVERDIR=$(O)

MIDL_FLAGS=-oldnames

SOURCES= \
	..\relstat.idl \
        service.c \
        rssvc.c \
        rssvc.rc \
        $(O)\relstat_s.c \
	
USE_MSVCRT=1

UMTYPE=console

TARGETLIBS= \
    $(SDK_LIB_PATH)\rpcrt4.lib \
    $(SDK_LIB_PATH)\ntdll.lib \
    $(SDK_LIB_PATH)\user32.lib
