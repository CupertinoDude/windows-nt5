TARGETNAME=memsnap
TARGETPATH=obj
TARGETTYPE=UMAPPL_NOLIB
INCLUDES=.;$(BASE_INC_PATH)

SOURCES=

USE_NATIVE_EH=1

USE_MSVCRT=1

MISCFILES = \
     memsnap.doc

UMTYPE=console
UMAPPL=memsnap*sortlog*poolsnap*poolfilt*memfilt
UMRES=$(@R).res
UMLIBS=$(SDK_LIB_PATH)\user32.lib \
       $(SDK_LIB_PATH)\ntdll.lib

NTTARGETFILES=$(O)\memsnap.res \
              $(O)\sortlog.res \
              $(O)\poolsnap.res \
              $(O)\poolfilt.res \
              $(O)\memfilt.res
