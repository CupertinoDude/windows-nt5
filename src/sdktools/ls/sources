TARGETNAME=ls
TARGETPATH=obj
TARGETTYPE=PROGRAM

USE_MSVCRT=1

INCLUDES=$(PROJECT_ROOT)\ztools\inc

SOURCES=ls.c ls.rc

C_DEFINES=-D_OS2_20_=0 -Dnear= -Dfar= -Dpascal=

UMTYPE=console
TARGETLIBS=\
    $(PROJECT_ROOT)\ztools\src\$(O)\ztools.lib \
    $(SDK_LIB_PATH)\user32.lib

MSC_WARNING_LEVEL=/W3 /WX
