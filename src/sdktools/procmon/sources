USE_MSVCRT=1

TARGETNAME=procmon
TARGETPATH=$(_OBJ_DIR)
TARGETTYPE=PROGRAM

UMENTRY=main

UMTYPE=console

SOURCES=procmon.c \
        procmon.rc

TARGETLIBS=$(BASEDIR)\public\sdk\lib\*\user32.lib \
           $(BASEDIR)\public\sdk\lib\*\kernel32.lib \
           $(BASEDIR)\public\sdk\lib\*\ntdll.lib
