TARGETNAME=SysParse
TARGETPATH=obj
TARGETTYPE=PROGRAM

UMTYPE=windows
UMENTRY=winmain

INCLUDES=.\; \

C_DEFINES=-DUSE_REGEXP -DWIN32

LINKER_FLAGS=/SWAPRUN:CD /SWAPRUN:NET

SOURCES = sysparse.cpp \
        sysparse.rc \
        cmdline.cpp \
        geninfo.cpp \
        logger.cpp \
        devwalk.cpp \
        genapwlk.cpp \
        nt4dev.cpp \
        nt5net.cpp

TARGETLIBS= \
    $(SDK_LIB_PATH)\version.lib \
    $(SDK_LIB_PATH)\rpcrt4.lib \
    $(SDK_LIB_PATH)\ole32.lib \
    $(SDK_LIB_PATH)\uuid.lib
