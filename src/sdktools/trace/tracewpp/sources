MAJORCOMP=sdktools
MINORCOMP=tracewpp

TARGETNAME=tracewpp
TARGETPATH=$(_OBJ_DIR)
TARGETTYPE=PROGRAM

MSC_WARNING_LEVEL = /W4 /WX /Zc:forScope

INCLUDES=$(BASEDIR)\public\sdk\inc
GPSIZE=32

SOURCES=tracewpp.cpp ezparse.cpp crc32.c tracewpp.rc tpl.cpp md5c.c

USE_CRTDLL=1
USE_STL=1
USE_NATIVE_EH=1

UMTYPE=console
UMLIBS= \
        $(BASEDIR)\public\sdk\lib\*\rpcrt4.lib 

