TARGETNAME=tracelog
TARGETPATH=obj
TARGETTYPE=PROGRAM

C_DEFINES=/DUNICODE /D_UNICODE

SOURCES=tracelog.c\
        tracelog.rc

UMTYPE=console
USE_MSVCRT=1

TARGETLIBS=$(SDK_LIB_PATH)\user32.lib \
        $(SDK_LIB_PATH)\shell32.lib \
        $(SDK_LIB_PATH)\ntdll.lib \
        $(SDK_LIB_PATH)\version.lib \
        $(SDK_LIB_PATH)\advapi32.lib

INCLUDES=$(INCLUDES);       \
         $(BASE_INC_PATH);
