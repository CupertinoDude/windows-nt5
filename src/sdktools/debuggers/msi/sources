!IF 0

This project contains files that are not built by the NT Build Lab.
This file is a template for all sources files within this project.

!ENDIF

TARGETNAME=
TARGETTYPE=NOTARGET
TARGETPATH=

!if $(386)
NTTARGETFILES=i386\dbgx.msi
!elseif $(IA64)
NTTARGETFILES=ia64\dbgx.msi
!endif

SOURCES =

# Files that have the same name, but are different for each platform

MISCFILES=                \
    dbgdata.txt           \
    dbginstall.cmd        \
    dbgx.msm              \
    dbgx.sed              \
    list.txt              \
    msizap.exe

I386_MISCFILES =          \
    i386\dbgx.msi         \
    i386\dbgemx.msm

IA64_MISCFILES =          \
    ia64\dbgx.msi


!if $(386)
MISCFILES=                \
    $(MISCFILES)          \
    $(I386_MISCFILES)

!elseif $(IA64)
MISCFILES=                \
    $(MISCFILES)          \
    $(IA64_MISCFILES)
!endif
