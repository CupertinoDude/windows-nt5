TARGETNAME=tc
TARGETPATH=obj
TARGETTYPE=PROGRAM

INCLUDES=$(PROJECT_ROOT)\ztools\inc

SOURCES=tc.c tc.rc

C_DEFINES=-D_OS2_20_=0 -Dnear= -Dfar= -Dpascal=

UMTYPE=console
UMLIBS=$(PROJECT_ROOT)\ztools\src\$(O)\ztools.lib \
       $(SDK_LIB_PATH)\user32.lib

USE_MSVCRT=1

