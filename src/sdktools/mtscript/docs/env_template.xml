<Environment xmlns="x-schema:enviro_schema.xml">
   <LongName>Blue Lab</LongName>
   <Description>Blue Virtual Lab's 3 x86 build machines</Description>
   <BuildManager Name="x86BldMgr1"/>
   <Options EmailAliasTo="bluelabmgrs"
            EmailAliasCC="bluelabteam"
            fIsLab="1"/>
   <Machine Name="x86BlueBld0" Enlistment="d:\newnt">
      <Depot type="array">Admin</Depot>
   </Machine>
   <Machine Name="x86BlueBld1" Enlistment="d:\newnt">
      <Depot>Root</Depot>
      <Depot>Base</Depot>
      <Depot>InetCore</Depot>
   </Machine>
   <Machine Name="x86BlueBld2" Enlistment="e:\newnt">
      <Depot>COM</Depot>
      <Depot>Drivers</Depot>
      <Depot>DS</Depot>
      <Depot>EndUser</Depot>
   </Machine>
   <Machine Name="x86BlueBld3" Enlistment="d:\newnt">
      <Depot>InetSrv</Depot>
      <Depot>MultiMedia</Depot>
      <Depot>Net</Depot>
      <Depot>PrintScan</Depot>
      <Depot>SDKTools</Depot>
      <Depot>Shell</Depot>
      <Depot>TermSrv</Depot>
      <Depot>Windows</Depot>
   </Machine>
</Environment>
