TARGETNAME=hhctrlui
TARGETPATH=obj
TARGETTYPE=DYNLINK

UMTYPE=windows

DLLDEF=hhctrl.def

PASS0_HEADERDIR=$(O)
PASS0_SOURCEDIR=$(O)

MIDL_TLBDIR=$(O)

USECXX_FLAG=-TP

C_DEFINES=$(C_DEFINES) -DHHCTRL -D_WINDLL -D_ATL_NO_UUIDOF -D_MIDL_USE_GUIDDEF_
C_DEFINES=$(C_DEFINES) -DOEMRESOURCE

!if $(FREEBUILD)
C_DEFINES=$(C_DEFINES) -DNDEBUG
!else
C_DEFINES=$(C_DEFINES) -D_DEBUG -D_CRTDBG_MAP_ALLOC
!endif

INCLUDES=$(SDK_INC_PATH);..;

RESOURCE_ONLY_DLL=1

SOURCES=..\hhfind.idl \
        ..\hhifc.idl  \
        ..\hhifc.idl  \
        ..\hhsort.idl \
        ..\hhctrl.rc
