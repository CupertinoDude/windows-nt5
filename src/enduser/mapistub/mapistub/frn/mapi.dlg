    EDITTEXT        IDD_EDIT_PPST_NAME, 106, 80, 110, 12, ES_AUTOHSCROLL
    PUSHBUTTON      "Pa&rcourir...", IDD_BROWSE_PPST, 220, 80, 40,14, WS_GROUP | WS_TABSTOP
    
// Startup page...
