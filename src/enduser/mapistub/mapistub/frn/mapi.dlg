//
//  SMTP Per-recipient send options
//
IDD_SMTPOPT_ENCODING DIALOG DISCARDABLE 0, 0, 277, 182
STYLE DS_MODALFRAME | WS_POPUP | WS_VISIBLE | WS_CAPTION | WS_SYSMENU
CAPTION ""
FONT SHELL_FONT_SIZE, SHELL_FONT
BEGIN

	CONTROL			"Je &veux spécifier le format des messages adressés à ce destinataire.",IDD_SMTPOPT_CARE,
					"Button", BS_AUTOCHECKBOX | WS_GROUP | WS_TABSTOP,
					15, 15, 258, 10

	LTEXT			"Format du message :",-1,15, 28, 80, 10, WS_DISABLED

	CONTROL			"&MIME",IDD_SMTPOPT_MIME,
					"Button", BS_AUTORADIOBUTTON | WS_GROUP | WS_TABSTOP | WS_DISABLED,
					23, 40, 164, 10

	CONTROL			"&Texte brut/UUEncode",IDD_SMTPOPT_NonMIME,
					"Button", BS_AUTORADIOBUTTON | WS_DISABLED,
					23, 109, 120, 10


//
//  Associated with the MIME selection
//
	CONTROL				"Te&xte brut",IDD_SMTPOPT_Text,
						"Button",BS_AUTORADIOBUTTON | WS_GROUP | WS_TABSTOP | WS_DISABLED,
						31, 62, 104, 10

	CONTROL				"&Inclure à la fois un format Texte brut et un format HTML",IDD_SMTPOPT_Both,
						"Button",BS_AUTORADIOBUTTON | WS_DISABLED,
						31, 73, 227, 10

	CONTROL				"&HTML",IDD_SMTPOPT_HTML,
						"Button",BS_AUTORADIOBUTTON | WS_DISABLED,
						31, 86, 60, 10

//
//  Associated with "Plain Text/UUEncode"
//
	CONTROL				"&BINHEX",IDD_SMTPOPT_BinHex,
						"Button",BS_AUTOCHECKBOX | WS_GROUP | WS_TABSTOP | WS_DISABLED,
						31,135,40,10

	GROUPBOX		"Corps du  message :",IDD_SMTPOPT_Body,23, 51, 238, 50, WS_DISABLED
	GROUPBOX		"Format de pièce jointe pour les fichiers Macintosh",IDD_SMTPOPT_Attach,23, 120, 238, 42, WS_DISABLED
END

