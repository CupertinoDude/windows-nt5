HKLM,%KEY_UNINSTALL%,"DisplayName",,"%PRODUCT_NAME_AND_VER%"
HKLM,%KEY_UNINSTALL%,"UninstallString",,"RunDll32 advpack.dll,LaunchINFSection %17%\msnetmtg.inf,NetMtg.Remove.W95"
#if defined(IE_ADDON)
HKLM,%KEY_UNINSTALL%,"QuietUninstallString",,"RunDll32 advpack.dll,LaunchINFSection %17%\msnetmtg.inf,NetMtg.Remove.W95,5"
#endif // defined(IE_ADDON)


// This flag tells IE4 setup that it is safe to leave NM on a system when
// uninstall IE4 if IE 3.0 was present before IE4 was installed.  It is used
// by the full install as well as the add-on install.  This is to protect
// against NetMeeting being left on the machine with a version of COMCTL32.DLL
// that doesn't have the features that we rely on.
HKLM,%KEY_UNINSTALL%,"RequiresIESysFile",,"4.71"

[NetMtg.Register.Remove.NT]
HKLM,%KEY_UNINSTALL%,"DisplayName",,"%PRODUCT_NAME_AND_VER%"
HKLM,%KEY_UNINSTALL%,"UninstallString",,"RunDll32 advpack.dll,LaunchINFSection %17%\msnetmtg.inf,NetMtg.Remove.NT"
#if defined(IE_ADDON)
HKLM,%KEY_UNINSTALL%,"QuietUninstallString",,"RunDll32 advpack.dll,LaunchINFSection %17%\msnetmtg.inf,NetMtg.Remove.NT,5"
#endif // defined(IE_ADDON)

// This flag tells IE4 setup that it is safe to leave NM on a system when
// uninstall IE4 if IE 3.0 was present before IE4 was installed.  It is used
// by the full install as well as the add-on install.  This is to protect
// against NetMeeting being left on the machine with a version of COMCTL32.DLL
// that doesn't have the features that we rely on.
HKLM,%KEY_UNINSTALL%,"RequiresIESysFile",,"4.71"

