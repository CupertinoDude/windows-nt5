TERMSRV_ROOT = ..\..\..
TSCOMMON_ROOT = ..\..
TSSEC_ROOT = ..

!include $(TSCOMMON_ROOT)\common.inc
!include $(TSCOMMON_ROOT)\win32com.inc

MAJORCOMP=TShrClnt
MINORCOMP=tssec

TARGETNAME=tssec
TARGETPATH=$(TERMSRV_ROOT)\lib\$(_OBJ_DIR)
TARGETTYPE=LIBRARY

TARGETLIBS= \
    $(CLIENT_LIB_DLL) \
    $(CLIENT_LIB_EXE) \
    $(SDK_LIB_PATH)\ntdll.lib

!include ..\tssec.inc

C_DEFINES=$(C_DEFINES) -DNOGENRANDOM
