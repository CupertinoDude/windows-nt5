TERMSRV_ROOT = ..\..\..\..\..
COMMON_ROOT = ..\..\..\..
LICENSE_ROOT = ..\..\..

MAJORCOMP=console
MINORCOMP=license

TARGETNAME=liclient
TARGETPATH=$(TERMSRV_ROOT)\lib\$(_OBJ_DIR)
TARGETTYPE=LIBRARY

INCLUDES=$(INCLUDES); \
         $(LICENSE_ROOT)\inc; \
         $(TERMSRV_ROOT)\common\tssec; \
         $(TERMSRV_ROOT)\common\inc; \
         $(TERMSRV_ROOT)\inc; \
         $(DS_INC_PATH)\crypto

C_DEFINES=$(C_DEFINES) -DOS_WINNT -DTESTING

!include ..\liclient.inc
