MAJORCOMP=console
MINORCOMP=license

C_DEFINES=$(C_DEFINES)-DOS_WINNT -DOS_WINCE

!if "$(WINCEDEBUG)"=="debug"
C_DEFINES=$(C_DEFINES) -DDBG=1
!endif

TERMSRV_ROOT = ..\..\..\..\..
TSHRCLNT_ROOT = ..\..\..\..\..\newclient
LICENSE_ROOT=..\..\..

TARGETNAME=debuglib
TARGETPATH=$(LICENSE_ROOT)\lib\wince\$(_CPUINDPATH)
TARGETTYPE=LIBRARY
RELEASETYPE=CUSTOM

INCLUDES=$(LICENSE_ROOT)\inc;\
         $(TERMSRV_ROOT)\inc;

!include ..\debuglib.inc

CDEFINES=$(C_DEFINES)
