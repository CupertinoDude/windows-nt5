MAJORCOMP=license
MINORCOMP=asn1

C_DEFINES=$(C_DEFINES) -DOS_WINNT

TERMSRV_ROOT = ..\..\..\..\..\..
LICENSE_ROOT = ..\..\..\..

TARGETNAME=asn1
TARGETPATH=$(TERMSRV_ROOT)\lib\$(_OBJ_DIR)
TARGETTYPE=LIBRARY



INCLUDES=..\..\include; \
         $(LICENSE_ROOT)\inc; \
         $(SDK_INC_PATH)\crt; \

!include ..\asn1.inc
