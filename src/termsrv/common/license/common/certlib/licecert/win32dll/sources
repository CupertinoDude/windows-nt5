MAJORCOMP=license
MINORCOMP=licecert

C_DEFINES=$(C_DEFINES) -DOS_WINNT -DLICECERT_DLL
CDEFINES=$(C_DEFINES)

TERMSRV_ROOT = ..\..\..\..\..\..
COMMON_ROOT = ..\..\..\..\..
LICENSE_ROOT = ..\..\..\..

TARGETNAME=licecrtd
TARGETPATH=$(TERMSRV_ROOT)\lib\$(_OBJ_DIR)
TARGETTYPE=LIBRARY

INCLUDES= \
    ..\..\include; \
    $(LICENSE_ROOT)\inc; \
    $(COMMON_ROOT)\inc; \
    $(TERMSRV_ROOT)\inc; \
    $(DS_INC_PATH)\crypto

!include ..\licecert.inc
