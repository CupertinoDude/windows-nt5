!ifndef ROOT
ROOT=$(MAKEDIR:\t120\common=)
!endif
!ifndef DEVROOT
DEVROOT=$(ROOT)\dev
!endif

!include $(DEVROOT)\common.inc

MAJORCOMP=t120
MINORCOMP=common

TARGETNAME=common
TARGETPATH=$(_OBJ_DIR)
TARGETTYPE=LIBRARY

SYNCHRONIZE_BLOCK=1

INCLUDES=$(INCLUDES);$(ROOT)\h\t120;$(ROOT)\t120\h;..;$(ROOT)\asn1\h;$(ROOT)\h

C_DEFINES=$(C_DEFINES) -D_WINDOWS
# -DJASPER

# Always use intrinsics, even in debug
USER_C_FLAGS=/Oi

386_STDCALL=1

LINKLIBS=\
        $(ROOT)\nmutil\nmutila\$(_OBJ_DIR)\*\nmutila.lib

#
# List of sources
#

SOURCES= \
        appcap.cpp \
        capid.cpp \
        t120dbg.cpp \
        memory.cpp \
        sesskey.cpp \
        objkey.cpp \
        regkey.cpp \
        cntlist.cpp \
        ms_util.cpp


#
# Precompiled specs
#

PRECOMPILED_INCLUDE = precomp.h
PRECOMPILED_PCH = precomp.pch
PRECOMPILED_OBJ = precomp.obj
PRECOMPILED_CXX = 1

CONDITIONAL_INCLUDES =  \
macocidl.h                              \
macwin32.h                              \
rpcerr.h                                \
rpcmac.h                                \
winwlm.h                                \
macname1.h                              \
