ALT_PROJECT=HYDRA
ALT_PROJECT_TARGET=.

TERMSRV_ROOT = ..\..

MAJORCOMP=termsrv
MINORCOMP=winsta

TARGETNAME=winsta
TARGETPATH=obj
TARGETTYPE=DYNLINK

PASS1_PUBLISH= \
    {$(O)\winsta.lib=$(SDK_LIB_PATH)\winsta.lib}

DLLENTRY=DllInitialize

TARGETLIBS= \
       $(TERMSRV_ROOT)\lib\$(O)\apisub.lib \
       $(SDK_LIB_PATH)\rpcrt4.lib   \
       $(SDK_LIB_PATH)\kernel32.lib \
       $(SDK_LIB_PATH)\advapi32.lib \
       $(SDK_LIB_PATH)\user32.lib

DLLBASE=0x30010000

MSC_WARNING_LEVEL=/W3 /WX

SOURCES=winsta.c \
        lscore.c \
        lcrpc-c.c \
        tsrpc-c.c \
        icarpc.c \
        ..\rpcwire.c \
        server.c \
        winsta.rc

INCLUDES = \
    ..; \
    ..\inc; \
    ..\rpc; \
    ..\inc\$(O)

C_DEFINES=-D_UNICODE -DUNICODE -DSECURITY_WIN32

# USE_MSVCRT=1

