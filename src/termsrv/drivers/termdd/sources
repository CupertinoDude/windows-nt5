ALT_PROJECT=HYDRA
ALT_PROJECT_TARGET=.

TERMSRV_ROOT = ..\..

MAJORCOMP=termsrv
MINORCOMP=termdd

NT_UP=0

TARGETNAME=termdd
TARGETPATH=$(TERMSRV_ROOT)\lib\$(_OBJ_DIR)
TARGETTYPE=EXPORT_DRIVER

TARGETLIBS=$(TARGETPATH)\*\ctxdd.lib

DLLDEF=icadd.def
INCLUDES = \
    ..\inc; \
    $(BASE_INC_PATH); \

SOURCES= \
        ntload.c   \
        beep.c     \
        buffer.c   \
        channel.c  \
        connect.c  \
        dispatch.c \
        icadata.c  \
        init.c     \
        keyboard.c \
        misc.c     \
        mouse.c    \
        stack.c    \
        timer.c    \
        video.c    \
        virtual.c  \
        trace.c    \
        khandle.c  \
        ptdrvcom.c \
        ptdrvstr.c \
        ptdrvkbd.c \
        ptdrvmou.c \
        ptdrvpnp.c \
        wmi.c      \
        icadd.rc

UMRES=$(O)\ica.res

PRECOMPILED_INCLUDE=precomp.h
PRECOMPILED_PCH=precomp.pch
PRECOMPILED_OBJ=precomp.obj
