MAJORCOMP=console
MINORCOMP=keypklib

TERMSRV_ROOT = ..\..\..
COMMON_ROOT = $(TERMSRV_ROOT)\COMMON

C_DEFINES=-DUNICODE

TARGETNAME=keypklib
TARGETPATH=obj
TARGETTYPE=LIBRARY

MSC_WARNING_LEVEL=$(MSC_WARNING_LEVEL) /W3 /WX

SOURCES=keypack.c

INCLUDES = \
    $(COMMON_ROOT)\license\inc;\
    $(DS_INC_PATH)\crypto

PRECOMPILED_INCLUDE=precomp.h
PRECOMPILED_PCH=precomp.pch
PRECOMPILED_OBJ=precomp.obj
