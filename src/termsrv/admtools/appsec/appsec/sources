MAJORCOMP=windows
MINORCOMP=shell

TARGETNAME=appsec
TARGETPATH=obj
TARGETTYPE=LIBRARY
TARGETLIBS=

!IFDEF NTDEBUG
!IFDEF NOMEMLEAK
C_DEFINES=-DCONDITION_HANDLING=1 -DDBG=1 -DUNICODE=0
!ELSE
!IFDEF STACK_TRACE
C_DEFINES=-DCONDITION_HANDLING=1 -DDBG=1 -DMEMLEAK -DSTACK_TRACE -DUNICODE=0
!ELSE
C_DEFINES=-DCONDITION_HANDLING=1 -DDBG=1 -DMEMLEAK -DUNICODE=0
!ENDIF
!ENDIF
!ELSE   # NTDEBUG
C_DEFINES=-DCONDITION_HANDLING=1 -DDBG=0 -DUNICODE=0
!ENDIF  # NTDEBUG

EXEPROFILEINPUT=yes

MSC_WARNING_LEVEL=/W3 /WX

SOURCES=appsec.cpp\
        commdlg.cpp\
        listctrl.cpp\
        adddlg.cpp\
        appsec.rc

UMTYPE=windows
UMAPPL=appsec
UMLIBS=$(SDK_LIB_PATH)\user32.lib \
       $(SDK_LIB_PATH)\comdlg32.lib \
       $(SDK_LIB_PATH)\ntdll.lib \
       $(TERMSRV_LIB_PATH)\utildll.lib \
       $(SDK_LIB_PATH)\mpr.lib \
        $(SDK_LIB_PATH)\comctl32.lib \
        $(SDK_LIB_PATH)\shell32.lib \
        obj\*\commdlg.obj \
        obj\*\listctrl.obj \
        obj\*\adddlg.obj \
        obj\*\appsec.res

NTTARGETFILE0=make_root
