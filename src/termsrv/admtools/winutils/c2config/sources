       $(SDK_LIB_PATH)\shell32.lib   \
       $(SDK_LIB_PATH)\netapi32.lib  \
       $(TERMSRV_LIB_PATH)\regapi.lib    \
       $(TERMSRV_LIB_PATH)\utildll.lib   \
       $(SDK_LIB_PATH)\winsta.lib
