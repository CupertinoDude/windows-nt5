!ifndef U_UNICODE
U_UNICODE=0
!endif

!if $(U_UNICODE)
C_DEFINES=-DUNICODE=1 -D_UNICODE=1
!endif

INCLUDES = .;..\inc

MAJORCOMP=ntreskit
MINORCOMP=c2config
GPSIZE=32
CHICAGO_PRODUCT=1

TARGETNAME=c2config
TARGETPATH=obj
TARGETTYPE=LIBRARY

SOURCES=c2config.c      \
        mainwnd.c       \
        listwnd.c       \
        titlewnd.c      \
        rebootex.c      \
        splash.c        \
        c2config.rc


UMENTRY=winmain
UMTYPE=windows
UMAPPL=c2config
UMLIBS=..\common\obj\*\c2lib.lib \
        obj\*\c2config.lib       \
        obj\*\c2config.res      \
        $(SDK_LIB_PATH)\shell32.lib
