!ifndef U_UNICODE
U_UNICODE=0
!endif

!if $(U_UNICODE)
C_DEFINES=-DUNICODE=1 -D_UNICODE=1
!endif

MAJORCOMP=ntreskit
MINORCOMP=c2acls

TARGETNAME=c2acls
TARGETPATH=obj
TARGETTYPE=DYNLINK
TARGETLIBS=..\common\$(O)\c2lib.lib                \
           $(SDK_LIB_PATH)\user32.lib   \
           $(SDK_LIB_PATH)\kernel32.lib \
           $(SDK_LIB_PATH)\gdi32.lib    \
           $(SDK_LIB_PATH)\samlib.lib   \
           $(SDK_LIB_PATH)\advapi32.lib \

INCLUDES = .;..\inc
CHICAGO_PRODUCT=1
DLLENTRY=DLLInit
USE_MSVCRT=1

SOURCES=dllinit.c   \
        aclfuncs.c  \
        c2regacl.c  \
        c2ntfacl.c  \
        c2acls.rc

DLLBASE=0x1000000
