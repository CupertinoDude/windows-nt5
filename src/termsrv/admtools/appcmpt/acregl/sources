TERMSRV_ROOT = ..\..\..

MAJORCOMP=termsrv

MINORCOMP=acregl

TARGETNAME=acregl
TARGETPATH=obj
TARGETTYPE=LIBRARY

# Mark as not TS aware, so we can make sure the per-user windows directory exists
NOT_TERMINAL_SERVER_AWARE=1

SOURCES=acregl.c \
        acregl.rc

INCLUDES=..\inc;..\..\inc;

!IFDEF NTDEBUG
!IFDEF NOMEMLEAK
C_DEFINES=-DCONDITION_HANDLING=1 -DNOMINMAX -DDBG=1 -DUNICODE=1
!ELSE
!IFDEF STACK_TRACE
C_DEFINES=-DCONDITION_HANDLING=1 -DNOMINMAX -DDBG=1 -DMEMLEAK -DSTACK_TRACE -DUNICODE=1
!ELSE
C_DEFINES=-DCONDITION_HANDLING=1 -DNOMINMAX -DDBG=1 -DMEMLEAK -DUNICODE=1
!ENDIF
!ENDIF
!ELSE   # NTDEBUG
C_DEFINES=-DCONDITION_HANDLING=1 -DNOMINMAX -DDBG=0 -DUNICODE=1
!ENDIF  # NTDEBUG


UMLIBS=$(O)\acregl.lib \
       ..\..\utilsub\$(O)\utilsub.lib \
       $(TERMSRV_LIB_PATH)\syslib.lib \
       $(SDK_LIB_PATH)\user32.lib \
       $(SDK_LIB_PATH)\shfolder.lib \
       $(SDK_LIB_PATH)\ntdll.lib

UMRES=$(O)\acregl.res
UMAPPLEXT=.exe
UMAPPL=acregl
UMTYPE=console
USE_MSVCRT=1

PRECOMPILED_INCLUDE=precomp.h
PRECOMPILED_PCH=precomp.pch
PRECOMPILED_OBJ=precomp.obj
