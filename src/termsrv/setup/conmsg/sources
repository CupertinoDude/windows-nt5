MAJORCOMP=ConMsg
MINORCOMP=ConMsg

TARGETPATH=obj
TARGETTYPE=PROGRAM
TARGETNAME=ConMsg

#C_DEFINES=-DWIN32 -D_UNICODE -DUNICODE -DWINDOWS
C_DEFINES=-DWIN32 -DWINDOWS

SOURCES=     ConMsg.cpp \
             stdafx.cpp   \
             ConMsg.rc


TARGETLIBS=$(SDK_LIB_PATH)\user32.lib \
           $(SDK_LIB_PATH)\wtsapi32.lib \
           $(SDK_LIB_PATH)\winsta.lib



UMTYPE=windows
UMENTRY=winmain

386_WARNING_LEVEL=-W4



