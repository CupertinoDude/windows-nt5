TARGETNAME=wmi
TARGETTYPE=DYNLINK
TARGETPATH=obj

DLLDEF=wmi.def

LINKLIBS= ..\advapi32\$(O)\advapip.lib
SYNCHRONIZE_DRAIN=1    # Make sure advapip.lib is built.

NTTARGETFILES=$(O)\wmip.lib

SOURCES=wmi.rc

UMTYPE=windows

PASS1_PUBLISH={$(O)\wmip.lib=$(SDK_LIB_PATH)\wmip.lib}
NO_BROWSER_FILE=1