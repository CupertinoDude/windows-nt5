SubClass=StillImage
DeviceType=3
DeviceSubType=0x1
Capabilities=0x00000031
DeviceData=Intel.DeviceData
ICMProfiles="sRGB Color Space Profile.icm"

[DefaultUninstall]
;DelFiles=   Intel.CopyDrivers,Intel.CopyExtensionDLL,Intel.CopyTWAIN
