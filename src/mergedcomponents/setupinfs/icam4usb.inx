[DestinationDirs]
Intel.CopySystem=10,system32\drivers
Intel.CopyDrivers=10,system32\drivers
Intel.CopyExtensionDLL=11
Intel.CopyTWAIN=10,twain_32\intel\usb

;---------------------------------------------------------------
