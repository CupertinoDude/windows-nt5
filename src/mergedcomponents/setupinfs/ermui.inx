; ER MUI INF
; This INF is used to install ER related components for MUI

[version]
signature="$Windows NT$" 
AdvancedINF=2.5

[DefaultInstall]
CopyFiles=ER.install,Inf.install

[SourceDisksNames]
55="",,0

[SourceDisksFiles]
ermui.inf=55

[Uninstall]
DelFiles=ER.install,Inf.install

[Inf.install]
ermui.inf

[DestinationDirs]
ER.install=11,%DWLCID%
Inf.install=10,mui\FALLBACK\%LCID%