MAJORCOMP=INTLWB
MINORCOMP=thawbrkr

SYNCHRONIZE_DRAIN=1

TARGETNAME=thawbrkr
TARGETPATH=obj
TARGETTYPE=DYNLINK

!IF  "$(THAIDBG)" == "1"
C_DEFINES=$(C_DEFINES) -DTHAIDBG
!ENDIF

!IF  "$(THAIINDEX)" == "1"
C_DEFINES=$(C_DEFINES) -DTHAIINDEX
!ENDIF


RCOPTIONS=-l 41e

USE_MSVCRT=1

!IFNDEF MSC_WARNING_LEVEL
MSC_WARNING_LEVEL=/W3
!ENDIF
MSC_WARNING_LEVEL=$(MSC_WARNING_LEVEL) /WX

INCLUDES=$(BASEDIR)\public\sdk\inc

SOURCES=thawbrkr.rc   \
        wbclassf.cxx  \
        exports.cxx   \
        iwbreak.cxx   \
        stemcf.cxx    \
        stemmer.cxx   \
        register.cxx  \
        ctplus0.cxx   \
        init.cxx
        

SUBSYSTEM_VERSION=4.00

UMTYPE=windows

DLLBASE=0x44e00000

DLLENTRY=DllMain

DLLDEF=$(O)\thawbrkr.def

PRECOMPILED_INCLUDE=pch.cxx

CONDITIONAL_INCLUDES= \
        $(CONDITIONAL_INCLUDES) \

LINKER_FLAGS=$(LINKER_FLAGS) -IGNORE:4077 

LINKLIBS=$(LINKLIBS)\
         $(SDK_LIB_PATH)\ntdll.lib  \
         $(SDK_LIB_PATH)\user32.lib\
         $(SDK_LIB_PATH)\kernel32.lib\
         $(SDK_LIB_PATH)\advapi32.lib \
         $(SDK_LIB_PATH)\uuid.lib\
         $(SDK_LIB_PATH)\ntquery.lib  \
         $(SDK_LIB_PATH)\usp10.lib

TARGETLIBS = $(TARGETLIBS) \
             ..\sth\$(_OBJ_DIR)\*\sth.lib\

MISCFILES=noise.tha
