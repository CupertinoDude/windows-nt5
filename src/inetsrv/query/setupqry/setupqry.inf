[Version]
Signature= "$Windows NT$"

;
; Installation .inf for Indexing Service v3.0
;
;
; this file is the inf that drives the fax setup
; during guimode and in the stand alone setup case.
;
; if you add files to the SourceDisksFiles section
; in this file, you MUST also add the file to the
; layout.inx file in the setup\inf\win4\inf directory.
; failing to to so will cause a file copy error
; during guimode.
;
;
; History:  1/21/97    dlee      Created
;           11/24/97   mohamedn  modified for query
;           10/19/98   sundara   added IS_ENUM_ALLOWED
;
; This list of optional components is presented to the user
; for selection
;
[Optional Components]
IndexSrv_System

[indexsrv_languages]
indexsrv_language_western_european
indexsrv_language_japanese_default
indexsrv_language_chinese_simplified
indexsrv_language_chinese_traditional
indexsrv_language_korean_default


[indexsrv_system]
OptionDesc = %indexsrv_systemOptionDesc%
Tip = %indexsrv_systemTip%
ISVersionMajor = 3
ISVersionMinor = 1
DelReg = UninstallDisableUSNLogReg
AddReg = ISReg, ISReg.Performance, indexsrv_language_neutral_reg, indexsrv_language_english_us_reg, indexsrv_language_english_uk_reg
IconIndex = 28
;Modes=0,1,2,3

[indexsrv_cisvc]
DisplayName = %cisvcDisplayName%
; share-process so system logon is enforced + interactive
ServiceType = 288
StartType = 3
ErrorControl = 1
ServiceBinary = %11%\cisvc.exe
Dependencies = RPCSS

[uninstall_indexsrv_system]
DelFiles = ISFiles, ISAdminHTMFiles, ISAdminIDQFiles, ISSSOFiles, ISPerfFiles, ISPDFFiles
DelReg = UninstallISReg, UninstallISReg.Performance, uninstall_indexsrv_language_neutral_reg, UninstallISReg.HTMLClsids
DelService = cisvc

[indexsrv_languages]
OptionDesc = %indexsrv_languageOptionDesc%
Tip = %indexsrv_languageTip%
Parent = indexsrv
IconIndex = 50

[indexsrv_language_western_european]
AddReg = indexsrv_language_english_us_reg, indexsrv_language_english_uk_reg, indexsrv_language_french_french,indexsrv_language_german_german,indexsrv_language_italian_italian,indexsrv_language_swedish_default,indexsrv_language_spanish_modern,indexsrv_language_dutch_dutch
LanguageName=Western_European

[indexsrv_language_english_us]
OptionDesc = %indexsrv_language_english_usOptionDesc%
Tip = %indexsrv_language_english_usTip%
Parent = indexsrv_language
IconIndex = 50
CopyFiles = indexsrv_language_english_us_files
