LINKLIBS=\
         ..\pch\$(_OBJ_DIR)\*\pch.obj\
         ..\pch\$(_OBJ_DIR)\*\pch.lib\
         $(LINKLIBS)

TARGETLIBS= \
         $(SDK_LIB_PATH)\query.lib\
         $(SDK_LIB_PATH)\uuid.lib \
         $(SDK_LIB_PATH)\user32.lib \
         $(SDK_LIB_PATH)\gdi32.lib \
         $(SDK_LIB_PATH)\comctl32.lib \
