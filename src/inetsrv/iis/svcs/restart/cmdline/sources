!include ..\..\..\place.inc

MAJORCOMP=iis
MINORCOMP=iisreset

TARGETNAME=iisreset
TARGETPATH=$(_OBJ_DIR)
TARGETTYPE=PROGRAM
UMTYPE=console
UNICODE=1
C_DEFINES=-DUNICODE -D_UNICODE

USE_MSVCRT=1

PASS0_HEADERDIR=$(O)
PASS0_SOURCEDIR=$(O)

TARGETLIBS=\
            $(SDK_LIB_PATH)\user32.lib   \
            $(SDK_LIB_PATH)\kernel32.lib \
            $(SDK_LIB_PATH)\ole32.lib

INCLUDES=..\sync; \
         $(IISBASEDIR)\inc; \
         $(IISBASEDIR)\inc\$(O);

SOURCES= \
        iisrstam.mc \
        iisrsta.rc \
        iisrsta.cxx
