!include ..\..\..\place.inc

MAJORCOMP=iisadmin
MINORCOMP=iisrstap

TARGETNAME=iisrstap
TARGETPATH=obj
TARGETTYPE=DYNLINK

DLLENTRY=DLLEntry

SYNCHRONIZE_BLOCK=1

INCLUDES=.;$(IISBASEDIR)\inc; \
         $(IISBASEDIR)\inc\$(O)

USE_MSVCRT=1
C_DEFINES=-DREGISTER_PROXY_DLL

TARGETLIBS= \
            $(SDK_LIB_PATH)\advapi32.lib \
            $(SDK_LIB_PATH)\kernel32.lib \
            $(SDK_LIB_PATH)\uuid.lib \
            $(SDK_LIB_PATH)\rpcrt4.lib \
            $(SDK_LIB_PATH)\user32.lib \
            $(SDK_LIB_PATH)\ole32.lib

SOURCES= \
    iisrsta.idl \
    dlldata.c\
    iisrsta_i.c \
    iisrsta_p.c \
    main.cxx \
    iisrsta.rc

UMTYPE=windows

PASS0_HEADERDIR=..\..\..\inc\$(O)
