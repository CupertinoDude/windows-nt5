!include ..\..\..\..\..\..\place.inc

MAJORCOMP=icrypt
MINORCOMP=server

TARGETNAME=exchsrv
TARGETPATH=obj
TARGETTYPE=PROGRAM

INCLUDES=.;..\inc;..\..\inc;..\..\..\..\..\..\inc

SOURCES=                \
        exchsrv.cxx     \
        exchsrv.rc      \

USE_MSVCRT=1

UMTYPE=console

TARGETLIBS=                                             \
        ..\..\lib\$(O)\testlib.lib                     \
        ..\..\..\..\..\lib\$(O)\iiscrypt.lib               \
        ..\..\..\..\..\lib\$(O)\icrypt.lib                 \
        $(IISBASEDIR)\svcs\iisrtl\$(O)\iisrtl.lib          \
        $(SDK_LIB_PATH)\user32.lib          \
        $(SDK_LIB_PATH)\ole32.lib           \
        $(SDK_LIB_PATH)\ws2_32.lib          \

PRECOMPILED_INCLUDE=precomp.hxx
PRECOMPILED_PCH=precomp.pch
PRECOMPILED_OBJ=precomp.obj
PRECOMPILED_CXX=1
