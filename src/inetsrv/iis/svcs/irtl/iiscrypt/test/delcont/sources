TARGETPATH=obj
TARGETTYPE=PROGRAM

INCLUDES=.;..\inc;..\..\..\..\..\inc

SOURCES=                \
        delcont.c       \
        delcont.rc      \

USE_MSVCRT=1

UMTYPE=console
UMAPPL=delcont

