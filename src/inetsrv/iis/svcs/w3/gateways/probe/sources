         ..\..\..\wam\object;\
         ..\..\..\wam\object\$(O);\
         ..\..\ext;\
         $(IISBASEDIR)\inc; \
         $(IISBASEDIR)\inc\$(O);

SOURCES=\
        probe.rc \
        main.cxx \
        worker.cxx \
        heapstat.cxx


