            $(SDK_LIB_PATH)\advapi32.lib    \
            $(SDK_LIB_PATH)\kernel32.lib    \
            $(SDK_LIB_PATH)\user32.lib      \
            $(IISBASEDIR)\svcs\iisrtl\$(O)\iisrtl.lib      \
            $(IISBASEDIR)\svcs\infocomm\info\server\$(O)\infocomm.lib    \


