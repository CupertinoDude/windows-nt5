MAJORCOMP=internet
MINORCOMP=www

TARGETNAME=crash
TARGETPATH=obj
TARGETTYPE=DYNLINK
TARGETLIBS= \
    $(SDK_LIB_PATH)\kernel32.lib  \
    $(SDK_LIB_PATH)\user32.lib

DLLBASE=0x00800000

USE_MSVCRT=1

INCLUDES=..\..\inc;..\inc;

SOURCES=      \
    crash.c   \
    crash.rc

UMTYPE=console
