DLLENTRY=DllLibMain
USE_MSVCRT=1


TARGETLIBS= \
    $(SDK_LIB_PATH)\kernel32.lib  \
    $(SDK_LIB_PATH)\user32.lib  \
    $(SDK_LIB_PATH)\advapi32.lib


INCLUDES=..\..\..\..\inc

SOURCES=	        \
	    bfwrite.c   \
	    bfwrite.rc  \

UMTYPE=console
