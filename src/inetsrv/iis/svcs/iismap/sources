!include ..\..\place.inc

MAJORCOMP=iismap
MINORCOMP=iismap

TARGETNAME=iismap
TARGETTYPE=DYNLINK
TARGETPATH=obj

MSC_WARNING_LEVEL=/W3 /WX

USE_MSVCRT=1
DLLENTRY=_DllMainCRTStartup

PASS0_HEADERDIR=$(O)
PASS0_SOURCEDIR=$(O)

TARGETLIBS=\
           ..\lib\$(O)\iiscrypt.lib \
           ..\lib\$(O)\icrypt.lib \
           $(IISBASEDIR)\svcs\iisrtl\$(O)\iisrtl.lib    \
           $(SDK_LIB_PATH)\kernel32.lib    \
           $(SDK_LIB_PATH)\advapi32.lib    \
           $(SDK_LIB_PATH)\user32.lib

SOURCES=\
        mapmsg.mc \
	iismap.cxx \
	md5c.c \
	iiscmr.cxx \
        memalloc.cxx \
	iismap.rc

INCLUDES=.\;..\..\inc;$(INCLUDES)


