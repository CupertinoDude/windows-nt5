!include ..\..\..\..\place.inc

MAJORCOMP=mdutil
MINORCOMP=mdutil

TARGETNAME=mdutil
TARGETPATH=obj
TARGETTYPE=PROGRAM

TARGETLIBS= \
            $(SDK_LIB_PATH)\user32.lib \
            $(SDK_LIB_PATH)\advapi32.lib    \
            $(SDK_LIB_PATH)\kernel32.lib \
            $(SDK_LIB_PATH)\ole32.lib \
            $(SDK_LIB_PATH)\uuid.lib \
            $(SDK_LIB_PATH)\rpcrt4.lib
	
	
INCLUDES=$(IISBASEDIR)\inc; \
         $(IISBASEDIR)\inc\$(O);

USE_MFC=1
USE_USRDLL=1

UMTYPE=console
UMENTRY=mainCRTStartup

SOURCES=main.cpp \
        admutil.cpp \
	tables.cpp \
	jd_misc.cpp\
	vptool.cpp\
	ansimeta.cpp\
	admutil.rc

