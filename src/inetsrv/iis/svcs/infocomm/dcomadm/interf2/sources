!include ..\..\..\..\place.inc

MAJORCOMP=iisadmin
MINORCOMP=coadmin

TARGETNAME=admwprox
TARGETPATH=obj
TARGETTYPE=DYNLINK
DLLENTRY=_DllMainCRTStartup

TARGETLIBS= \

INCLUDES=.;..\inc;..\..\..\..\inc

SYNCHRONIZE_BLOCK=1

USE_MSVCRT=1
#UNICODE=1
#NET_C_DEFINES=-DUNICODE
C_DEFINES=-DREGISTER_PROXY_DLL -DENTRY_PREFIX=Adm
WIN32_WINNT_VERSION=0x500

TARGETLIBS= \
    $(SDK_LIB_PATH)\advapi32.lib \
    $(SDK_LIB_PATH)\kernel32.lib \
    $(SDK_LIB_PATH)\uuid.lib \
    $(SDK_LIB_PATH)\rpcrt4.lib \
    $(SDK_LIB_PATH)\user32.lib \
    $(SDK_LIB_PATH)\ole32.lib \
    $(IISBASEDIR)\lib\$(O)\iisrtl2.lib \
    ..\..\..\lib\$(O)\iiscrypt.lib \
    ..\..\..\lib\$(O)\icrypt.lib   \

SOURCES= dlldata.c \
         iadmw_i.c \
         admwprox.rc \
         iadmwxp.c \
         main.cxx \
         security.cxx \
         secdat.cxx

UMTYPE=windows

NTTARGETFILE0= $(O)\iadmw_i.c \
               $(O)\iadmw_p.c \
               $(O)\dlldata.c \
               $(IISBASEDIR)\inc\$(O)\iadmw.h

PASS0_PUBLISH={$(IISBASEDIR)\inc\$(O)\iadmw.h=$(SDK_INC_PATH)\iadmw.h}
