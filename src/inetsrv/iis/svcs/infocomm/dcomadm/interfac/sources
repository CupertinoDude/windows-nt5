!include ..\..\..\..\place.inc

MAJORCOMP=iisadmin
MINORCOMP=coadmin

TARGETNAME=admprox
TARGETPATH=obj
TARGETTYPE=DYNLINK
DLLENTRY=DLLEntry

TARGETLIBS= \

INCLUDES=.;..\inc;..\..\..\..\inc


USE_MSVCRT=1
#UNICODE=1
#NET_C_DEFINES=-DUNICODE
C_DEFINES=-DREGISTER_PROXY_DLL

TARGETLIBS= \
            $(SDK_LIB_PATH)\advapi32.lib \
            $(SDK_LIB_PATH)\kernel32.lib \
            $(SDK_LIB_PATH)\uuid.lib \
            $(SDK_LIB_PATH)\rpcrt4.lib \
            $(SDK_LIB_PATH)\user32.lib \
            $(SDK_LIB_PATH)\ole32.lib \
            ..\..\..\lib\$(O)\iiscrypt.lib \
            ..\..\..\lib\$(O)\icrypt.lib   \
            ..\..\..\lib\*\isdebug.lib

SOURCES= dlldata.c \
         iadm_i.c \
         iadmxp.c \
         main.cxx \
         security.cxx \
         secdat.cxx \
         admprox.rc

UMTYPE=windows

NTTARGETFILE0=iadm_i.c \
              iadm_p.c \
              dlldata.c \
              $(BASEDIR)\public\sdk\inc\iadm.h

