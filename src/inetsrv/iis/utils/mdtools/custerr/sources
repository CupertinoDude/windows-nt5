!include ..\..\..\place.inc

MAJORCOMP=custerr
MINORCOMP=custerr

TARGETNAME=custerr
TARGETPATH=obj
TARGETTYPE=UMAPPL_NOLIB

INCLUDES=.;..\inc;..\..\..\inc

SOURCES=                \
        custerr.rc      \

USE_MSVCRT=1

UMTYPE=console
UMAPPL=custerr
UMENTRY=wmain

UMLIBS=                                                         \
            $(SDK_LIB_PATH)\user32.lib              \
            $(SDK_LIB_PATH)\advapi32.lib            \
            $(SDK_LIB_PATH)\kernel32.lib            \
            $(SDK_LIB_PATH)\ole32.lib               \
            $(SDK_LIB_PATH)\uuid.lib                \
            ..\guid\$(O)\guid.lib                              \
            ..\lib\$(O)\mdlib.lib                              \
            $(O)\custerr.res                                   \

PRECOMPILED_INCLUDE=precomp.hxx
PRECOMPILED_PCH=precomp.pch
PRECOMPILED_OBJ=precomp.obj
PRECOMPILED_CXX=1

