TARGETNAME=hclient
TARGETTYPE=PROGRAM
UMTYPE=windows
UMENTRY=winmain

MCS_WARNING_LEVEL= /W3/WX
USE_MFC=1

TARGETPATH=obj

TARGETLIBS=$(SDK_LIB_PATH)\hid.lib \
           $(SDK_LIB_PATH)\setupapi.lib \
           $(SDK_LIB_PATH)\comdlg32.lib

SOURCES= hclient.c \
         pnp.c \
         report.c \
         strings.c \
         logpnp.c \
         buffers.c \
         debug.c \
         ecdisp.c \
         extcalls.c \
         hclient.rc

