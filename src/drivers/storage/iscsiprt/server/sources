NT_SIGNCODE=1

TARGETNAME=iscsisrv
TARGETPATH=obj
TARGETTYPE=DRIVER

INCLUDES=$(BASE_INC_PATH);$(DDK_INC_PATH);$(BASEDIR)\base\ntos\inc;$(PUBLIC_INTERNAL_PATH)\net\inc;$(BASEDIR)\drivers\storage\iscsiprt\inc

TARGETLIBS=$(DDK_LIB_PATH)\ksecdd.lib \
           $(DDK_LIB_PATH)\tdi.lib

SOURCES= init.c \
         pnp.c  \
         dispatch.c \
         port.c     \
         internal.c \
         power.c    \
         wmi.c      \
         network.c  \
         utils.c    \
         lock.c     \
         protocol.c \
         sysutils.c \
         iscsiprt.rc


