<?xml version="1.0"?>
<AUTODISCOVERY>
	<USER>
		<DISPLAYNAME>Joe DisplayName User</DISPLAYNAME>
	</USER>
	<RESPONSE>
		<RESPONSEVER>0.1</RESPONSEVER>
		<ACCOUNT>
			<TYPE>EMail</TYPE>
			<ACTION>settings</ACTION>
			<PROTOCOL>
				<TYPE>POP3</TYPE>
				<SERVER>pop.mail.starbuck.com</SERVER>
				<PORT>987</PORT>
				<LOGINNAME>my_pop_loginname</LOGINNAME>
				<SSL>on</SSL>
				<SPA>on</SPA>
			</PROTOCOL>
			<PROTOCOL>
				<TYPE>SMTP</TYPE>
				<SERVER>smtp.mail.starbuck.com</SERVER>
				<PORT>678</PORT>
				<LOGINNAME>my_smpt_loginname</LOGINNAME>
				<SSL>off</SSL>
				<SPA>off</SPA>
				<AUTHREQUIRED>on</AUTHREQUIRED>
				<USEPOPAUTH>off</USEPOPAUTH>
			</PROTOCOL>
		</ACCOUNT>
	</RESPONSE>
</AUTODISCOVERY>
