CCSHELL_DIR     = $(PROJECT_ROOT)
TARGET_WIN95    = 1

!include ..\srcc.inc

SOURCES_USED    = $(SOURCES_USED) ..\srcc.inc

# target
TARGETNAME=badappsa

# The shell expects badappsa.lib to be in shell\lib\$O
# NT Setup expects badappsa.lib to be in internal\shell\lib\$(TARGET_DIRECTORY)\
NTTARGETFILES= $(CCSHELL_DIR)\lib\$O\badappsa.lib

# Only publish the free lib

!if "$(_OBJ_DIR)"=="obj"
PASS1_PUBLISH={$(TARGETPATH)\$(TARGET_DIRECTORY)\$(TARGETNAME).lib=$(PROJECT_LIB_PATH)\$(TARGETNAME).lib}
!endif
