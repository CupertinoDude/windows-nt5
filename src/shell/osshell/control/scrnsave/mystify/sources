TARGETNAME=ssmyst
TARGETPATH=obj
TARGETTYPE=UMAPPL_NOLIB
USE_MSVCRT=1

SXS_ASSEMBLY_NAME=Microsoft.Windows.Shell.ScreenSavers
SXS_ASSEMBLY_VERSION=1.0
SXS_ASSEMBLY_LANGUAGE_INDEPENDENT=1
SXS_MANIFEST=ScrnSave.manifest
SXS_MANIFEST_IN_RESOURCES=1
SXS_MANIFEST_RESOURCE_ID=1
SXS_NO_BINPLACE=1

INCLUDES=..\common;..\..\h;..\..\..\inc
INCLUDES        = $(INCLUDES);$(PROJECT_ROOT)\lib\$(O)

C_DEFINES=-DW3 -DUNICODE -D_UNICODE

!IFNDEF MSC_WARNING_LEVEL
MSC_WARNING_LEVEL=/W3
!ENDIF
MSC_WARNING_LEVEL=$(MSC_WARNING_LEVEL) /WX

SOURCES=mystify.rc

UMTYPE=windows
UMAPPL=ssmyst
UMAPPLEXT=.scr
UMLIBS=$(SDK_LIB_PATH)\scrnsavw.lib  \
       $(SDK_LIB_PATH)\comctl32.lib  \
       $(O)\mystify.res
