TARGETNAME = intl
TARGETTYPE = DYNLINK
TARGETPATH = obj
TARGETEXT  = cpl
TARGETLIBS = $(SDK_LIB_PATH)\kernel32.lib        \
             $(SDK_LIB_PATH)\kernl32p.lib        \
