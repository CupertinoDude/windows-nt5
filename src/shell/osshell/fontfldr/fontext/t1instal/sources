USE_MSVCRT=1

TARGETNAME=t1instal
TARGETPATH=obj
TARGETTYPE=LIBRARY

C_DEFINES=-DWIN32 -DW3 -DRELATIVESTEMS

!IFNDEF MSC_WARNING_LEVEL
MSC_WARNING_LEVEL=/W3
!ENDIF
MSC_WARNING_LEVEL=$(MSC_WARNING_LEVEL) /WX

PASS0_HEADERDIR=$(O)
PASS0_SOURCEDIR=$(O)

INCLUDES=$(O)

SOURCES=atmpfm.c    \
        builder.c   \
        charstr.c   \
        encoding.c  \
        errors.c    \
        freader.c   \
        fwriter.c   \
        hints.c     \
        makepfm.c   \
        mreader.c   \
        pfb.c       \
        pfm.c       \
        t1instal.c  \
        t1parser.c  \
        titott.c    \
        trans.c     \
        trig.c      \
        ttprog.c    \
        winio.c     \
        t1instal.rc \
	t1msg.mc
