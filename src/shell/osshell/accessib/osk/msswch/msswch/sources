TARGETNAME=msswch
TARGETPATH=obj
TARGETTYPE=DYNLINK
DLLENTRY = MSSwchDll_DllMain

USE_MSVCRT=1

INCLUDES=..\..\inc;..\..\..\inc

SOURCES=msswch.rc  \
        msswch.c   \
        swchcom.c  \
        swchjoy.c  \
        swchkey.c  \
        swchlist.c \
        swchlpt.c \
        mappedfile.c \
        kbdhook.c

TARGETLIBS=$(SDK_LIB_PATH)\kernel32.lib  \
           $(SDK_LIB_PATH)\user32.lib    \
           $(SDK_LIB_PATH)\advapi32.lib  \
           $(SDK_LIB_PATH)\winmm.lib

UMTYPE=console

