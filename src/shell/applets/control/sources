CCSHELL_DIR=$(PROJECT_ROOT)

TARGETNAME=ctrllib
TARGETPATH=obj
TARGETTYPE=LIBRARY

TARGETLIBS=

NOT_LEAN_AND_MEAN=1

USE_LIBCMT=1

C_DEFINES=-DUNICODE -D_UNICODE


SOURCES= \
         control.rc \
         init.c

UMTYPE=windows
UMAPPL=control

!IF $(386)
UMENTRYABS=ModuleEntry@0
!ELSE
UMENTRYABS=ModuleEntry
!ENDIF

UMRES=$(O)\control.res
UMLIBS= \
        $(SDK_LIB_PATH)\imm32.lib    \
        $(SDK_LIB_PATH)\shell32.lib    \
        $(SHELL_LIB_PATH)\shlwapip.lib \
        $(O)\ctrllib.lib
