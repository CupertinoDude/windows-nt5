CCSHELL_DIR=$(PROJECT_ROOT)

!include $(CCSHELL_DIR)\common.inc
SOURCES_USED=$(SOURCES_USED) $(CCSHELL_DIR)\common.inc

NO_BROWSER_FILE=1

USE_STATIC_ATL=1

PRECOMPILED_CXX=1
PRECOMPILED_INCLUDE=precomp.h
PRECOMPILED_OBJ=precomp.obj

TARGETNAME=sdspatch
TARGETPATH=obj
TARGETTYPE=LIBRARY

SOURCES= \
    sddebug.cpp  \
    sdflditm.cpp \
    sdfldits.cpp \
    sdfldits.cpp \
    sdfolder.cpp \
    sdlink.cpp   \
    sdmain.cpp   \
    sdwindow.cpp \
    sdutil.cpp

C_DEFINES=$(C_DEFINES) -DNT -DWIN32 -DWINNT -DUNICODE

