MAJORCOMP=tcpcmd
MINORCOMP=osdisp

TARGETNAME=osdisp
TARGETPATH=obj
TARGETTYPE=LIBRARY

USE_MSVCRT=1

NTOSDIR=$(PROJECT_ROOT)\ntos
INCLUDES=$(PROJECT_ROOT)\ntos\inc;$(PROJECT_ROOT)\boot\inc

C_DEFINES=-DSECURITY_WIN32 -DUNICODE

UNICODE=1

SOURCES= \
    osdisp.c

UMTYPE=console
UMAPPL=osdisp
UMLIBS=$(SDK_LIB_PATH)\ntdll.lib \
       $(SDK_LIB_PATH)\USER32.lib

