MAJORCOMP=remoteboot
MINORCOMP=rccclnt

TARGETNAME=rccclnt
TARGETPATH=obj
TARGETTYPE=DYNLINK

DLLENTRY=RCCClntInitDll
USE_MSVCRT=1

MSC_WARNING_LEVEL=/W3 /WX

UMTYPE=console

LINKLIBS= \
            $(SDK_LIB_PATH)\kernel32.lib \
            $(SDK_LIB_PATH)\ntdll.lib \
            $(SDK_LIB_PATH)\ws2_32.lib \
            $(SDK_LIB_PATH)\user32.lib

SOURCES = \
        rccclnt.c    \
        rccclnt.rc



