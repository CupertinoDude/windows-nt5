MAJORCOMP=tcpcmd
MINORCOMP=udplogin

TARGETNAME=udplogin
TARGETPATH=obj
TARGETTYPE=LIBRARY

USE_MSVCRT=1

INCLUDES=..\..\..\tcpcmd\common

