MAJORCOMP=ntos
MINORCOMP=efi

EFI_ROOT=$(PROJECT_ROOT)\efiutil\sdk
TARGETNAME=launcher
TARGETPATH=obj
TARGETTYPE=LIBRARY

CFLAGS=-entry:EfiMain


!if $(IA64)
INCLUDES=$(EFI_ROOT)\inc;$(EFI_ROOT)\inc\ia64;$(EFI_ROOT)\shell\inc
!else
INCLUDES=$(EFI_ROOT)\inc;$(EFI_ROOT)\inc\ia32;$(EFI_ROOT)\shell\inc
386_STDCALL=0
!endif


SOURCES=launcher.c \
        launcher.rc

NTTARGETFILES=$(O)\$(TARGETNAME).efi
