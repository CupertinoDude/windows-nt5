MAJORCOMP=      ntos
MINORCOMP=      tcsrv

TARGETNAME=     tcclnt
TARGETPATH=     obj
TARGETTYPE=     PROGRAM

UMTYPE=console
USE_MSVCRT= 1
C_DEFINES=$(C_DEFINES) -DSECURITY_WIN32 -DUNICODE -D_UNICODE
TARGETLIBS= $(SDK_LIB_PATH)\ntdll.lib \
            $(SDK_LIB_PATH)\ws2_32.lib\
            $(SDK_LIB_PATH)\user32.lib

MC_FLAGS=-v -A

      
INCLUDES= $(SDK_INC_PATH)\mfc42; \
          $(ADMIN_INC_PATH); \
          $(DS_INC_PATH); \
          ..\inc;
        
SOURCES= main.c\
         parser.c\
         console.c \
         tcclnt.rc 
