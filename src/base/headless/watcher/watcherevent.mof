class NETINFO
{
    sint32 ID;
    string ADDRESS;
};


class BLUESCREEN : __ExtrinsicEvent
{
    string MACHINE;
    string IPADDRESS[];
    string STOPCODE;
    [key]
    string GUID="";
};

instance of Win32PseudoProvider as $P1
{
    Name = "WatcherEventProv";
};

instance of __EventProviderRegistration
{
    Provider = $P1;
    EventQueryList = {"select * from WatcherEvent"};
};
