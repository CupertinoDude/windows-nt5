# component
MINORCOMP=OSUNINST
MAJORCOMP=setup

# target
TARGETNAME=OSUNINST
TARGETPATH=obj
TARGETTYPE=DYNLINK

# compiler options
USE_CRTDLL=1
C_DEFINES=$(C_DEFINES) -DUNICODE

# linker options
DLLDEF=undo.def
DLLENTRY=_DllMainCRTStartup

# source files
SOURCES=interface.c undo.rc

# binplace options
PASS1_PUBLISH={$(O)\$(TARGETNAME).lib=$(PROJECT_LIB_PATH)\$(TARGETNAME).lib}
