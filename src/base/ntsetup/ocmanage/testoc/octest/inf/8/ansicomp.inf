[version]
signature="$Windows NT$"
ClassGUID={00000000-0000-0000-0000-000000000000}

[Optional Components]
ansicomp
ansicomp_1
ansicomp_2
unicomp_1_1
unicomp_1_2

[ansicomp]
OptionDesc = %ansicomp_option_desc%
Tip        = %ansicomp_tip%
IconIndex  = 27

[ansicomp_1]
OptionDesc = %ansicomp_1_option_desc%
Tip        = %ansicomp_1_tip%
IconIndex  = *
Parent     = ansicomp

[ansicomp_2]
OptionDesc = %ansicomp_2_option_desc%
Tip        = %ansicomp_2_tip%
IconIndex  = *
Parent     = ansicomp

[unicomp_1_1]
OptionDesc = %unicomp_1_1_option_desc%
Tip        = %unicomp_1_1_tip%
IconIndex  = *
Parent     = unicomp_1

[unicomp_1_2]
OptionDesc = %unicomp_1_2_option_desc%
Tip        = %unicomp_1_2_tip%
IconIndex  = *
Parent     = unicomp_1


[ansicomp.ansicomp_1]
Bitmap     = bitmap6
Copyfiles  = ansicomp.ansicomp_1.copyfiles     

[ansicomp.ansicomp_2]
Bitmap     = bitmap7
Copyfiles  = ansicomp.ansicomp_2.copyfiles     

[ansicomp.unicomp_1_1]
Bitmap     = bitmap7
Copyfiles  = ansicomp.unicomp_1_1.copyfiles

[ansicomp.unicomp_1_2]
Bitmap     = bitmap7
Copyfiles  = ansicomp.unicomp_1_2.copyfiles


[ansicomp.ansicomp_1.copyfiles]
file5.txt

[ansicomp.ansicomp_2.copyfiles]
file6.txt

[ansicomp.unicomp_1_1.copyfiles]
file1.txt

[ansicomp.unicomp_1_2.copyfiles]
file2.txt


[DestinationDirs]
DefaultDestDir = 11, target

[SourceDisksNames]
1="Source Disk Name",,,

[SourceDisksFiles]
file1.txt=1,,10000000
file5.txt=1,,0x80000
file2.txt=1,,0x234324
file6.txt=1,,0x30000


[Strings]
ansicomp_option_desc     = "ANSI component "
ansicomp_1_option_desc   = "ANSI component _1 (needs ansicomp_2)"
ansicomp_2_option_desc   = "ANSI component _2 (needs unicomp_2_2)"
unicomp_1_1_option_desc = "ANSI-UNI component _1_1" 
unicomp_1_2_option_desc = "ANSI-UNI component _1_2"

ansicomp_tip            = "ansicomp"
ansicomp_1_tip          = "Child of ansicomp"
ansicomp_2_tip          = "Child of ansicomp"
unicomp_1_1_tip         = "Child of unicomp_1"
unicomp_1_2_tip         = "Child of unicomp_1"
