Copyfiles  = ansicomp.ansicomp_1.copyfiles     

[ansicomp.ansicomp_2]
Bitmap     = bitmap7
Copyfiles  = ansicomp.ansicomp_2.copyfiles     

[ansicomp.ansicomp_1_1]
Bitmap     = bitmap7
Copyfiles  = ansicomp.ansicomp_1_1.copyfiles

[ansicomp.ansicomp_1_2]
