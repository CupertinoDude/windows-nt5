    <META http-equiv="Content-Type" content="text/html; charset=iso-8859-1">

<script language=javascript>
function oemadd_LoadMe()
{
    window.parent.InitFrameRef();
    window.parent.InitNewButtons();
    document.all.btnNext.onclick = oemadd_NextBtnHandler;
    document.all.btnSkip.onclick = oemadd_SkipBtnHandler;
}

function oemadd_NextBtnHandler()
{
    window.parent.GoNavigate(window.parent.CKPT_ICONN);
}

function oemadd_SkipBtnHandler()
{
    window.parent.GoNavigate(window.parent.CKPT_ICONN);
}

</script>
</HEAD>

