    ..\..\inc;          \
    ..\inc;             \
    ..\..\ntos\inc;     \
    ..\clusnet\inc;     \
    $(PUBLIC_INTERNAL_PATH)\drivers\inc

SOURCES=clusdisk.c  \
        object.c    \
        clusdisk.rc \
        ezwrite.c

RUN_WPP=clusdisk.c ezwrite.c -ini:tracewpp.ini
