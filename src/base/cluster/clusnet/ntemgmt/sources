MAJORCOMP=ntos
MINORCOMP=clusnet

TARGETNAME=ntemgmt
TARGETTYPE=DRIVER_LIBRARY
TARGETPATH=$(_OBJ_DIR)

NT_UP=0

INCLUDES=\
    ..\inc;\
    ..\..\inc;\
    $(NET_INC_PATH);\
    $(PROJECT_ROOT)\ntos\inc

C_DEFINES=$(C_DEFINES) -D_NTDRIVER_

MSC_WARNING_LEVEL=/W3 /WX

SOURCES= \
    ntemgmt.c \
    nbtmgmt.c

RUN_WPP=$(SOURCES) -ini:..\inc\tracewpp.ini
