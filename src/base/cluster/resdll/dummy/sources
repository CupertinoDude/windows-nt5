!INCLUDE $(PROJECT_ROOT)\cluster\sources.inc
MINORCOMP=dummy

TARGETNAME=dummy
TARGETPATH=$(_OBJ_DIR)
TARGETTYPE=DYNLINK

MIDL_UUIDDIR=$(O)
PASS0_SOURCEDIR=$(O)
PASS0_HEADERDIR=$(O)
MIDL_TLBDIR=$(O)

INCLUDES=..\inc;..\..\inc

USE_MSVCRT=1
UNICODE=1
MSC_WARNING_LEVEL=/W4 /WX

SOURCES= \
        dummy.cpp    \
        dummy.rc

DLLBASE=0x7F220000
#UMTYPE=console
TARGETLIBS= $(SDK_LIB_PATH)\kernel32.lib \
            $(SDK_LIB_PATH)\advapi32.lib \
            $(SDK_LIB_PATH)\clusapi.lib \
            $(SDK_LIB_PATH)\resutils.lib \
            ..\..\clusrtl\$(O)\clusrtl.lib \
