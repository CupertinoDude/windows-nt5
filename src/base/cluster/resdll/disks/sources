!INCLUDE $(PROJECT_ROOT)\cluster\sources.inc
MINORCOMP=disks

TARGETNAME=disks
TARGETPATH=$(_OBJ_DIR)
TARGETTYPE=LIBRARY

INCLUDES=..\..\..\inc;..\inc;..\..\inc;..\..\inc\$(O);..\..\clusrtl;$(DRIVERS_INC_PATH)

USE_MSVCRT=1
UNICODE=1

#
# to generate browser info, set browser_info to 1 in your
# local environment. This also pulls in browser info from
# clusrtl since that is where the disk config classes originate
#
OTHER_SBR_FILES=..\..\clusrtl\$(O)\*.sbr
BSCMAKE_FLAGS=/n

SOURCES=disks.c     \
        filter.c    \
        scsi.c      \
        arbitrat.c  \
        newdisks.c  \
        pnp.c \
        newmount.c  \
        mountpoints.c   
