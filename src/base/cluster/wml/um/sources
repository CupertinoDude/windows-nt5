UMLIBS=	$(SDK_LIB_PATH)\user32.lib \
		$(SDK_LIB_PATH)\advapi32.lib \
		$(SDK_LIB_PATH)\kernel32.lib\
		$(SDK_LIB_PATH)\ntdll.lib

