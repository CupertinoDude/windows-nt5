<job id="Cluster Binary Property">

	<reference guid="{f2e606e0-2631-11d1-89f1-00a0c90d061e}" version="1.0" />

	<script language="VBScript">

		Option Explicit

		Dim oCluster
		Dim oProps
		Dim oProp
		Dim vDiskInfo

		Set oCluster = CreateObject("MSCluster.Cluster")

		oCluster.Open ("galenb-clus")
		MsgBox oCluster.Name

		set oProps = oCluster.Resources.Item( "Disk E:" ).PrivateProperties

		set oProp = oProps.Item( "DiskInfo" )

		oProp.UseDefaultValue

		vDiskInfo = oProp.Value

		oProp.Value = vDiskInfo

	</script>
</job>
