!INCLUDE $(PROJECT_ROOT)\cluster\sources.inc
MINORCOMP=leaks

TARGETNAME=leaks
TARGETPATH=$(_OBJ_DIR)
TARGETTYPE=DYNLINK

INCLUDES=.;..\..\inc;..\..\inc\$(O);..\..\sdk\inc

SOURCES=leaks.c   \
        leaks.rc

DLLBASE=0x00F90000
DLLENTRY=LeaksDllEntry
UMTYPE=windows
USE_MSVCRT=1

TARGETLIBS=..\..\clusrtl\$(O)\clusrtl.lib \
           $(SDK_LIB_PATH)\advapi32.lib \
           $(SDK_LIB_PATH)\kernel32.lib
