i386_SOURCES=$(i386_SOURCES)

C_DEFINES=$(C_DEFINES) -DELTORITO -D_WANT_MACHINE_IDENTIFICATION

!IF "$(NOLINK)" != "1"
!IF "$(ALT_PROJECT)" == "ArcI386"

NTTARGETFILES=$(O)\setupldr.lib $(O)\arcsetup$(DBG_LDR).exe
