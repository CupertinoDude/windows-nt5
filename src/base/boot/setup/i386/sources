!ELSE

NTTARGETFILES=$(O)\setupldr$(DBG_LDR).bin $(O)\setupldr.lib $(O)\setupldr$(DBG_LDR).exe
MISCFILES=$(O)\setupldr$(DBG_LDR).bin $(O)\setupldr$(DBG_LDR).exe
!ENDIF
!ENDIF
