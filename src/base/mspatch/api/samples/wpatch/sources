TARGETPATH=obj
TARGETNAME=wpatch
TARGETTYPE=PROGRAM

SOURCES=\
   wpatch.rc \
   wpatch.c

INCLUDES=..\..

USE_LIBC=1

!IFDEF TESTCODE
C_DEFINES=$(C_DEFINES) -DTESTCODE=$(TESTCODE)
USE_LIBC=
USE_CRTDLL=1
!ENDIF

!IFDEF DEBUG
C_DEFINES=$(C_DEFINES) -DDEBUG=$(DEBUG)
USE_LIBC=
USE_CRTDLL=1
!ENDIF

UMTYPE=windows
UMENTRY=winmain
TARGETLIBS=..\..\statica\$(O)\mspatchz.lib \
       $(SDK_LIB_PATH)\comctl32.lib \
       $(SDK_LIB_PATH)\user32.lib 

WINVER_VERSION=0x0400

MSC_WARNING_LEVEL=/W4
