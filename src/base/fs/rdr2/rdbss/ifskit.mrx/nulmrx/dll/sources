TARGETNAME=nulmrxnp
TARGETPATH=obj
TARGETTYPE=DYNLINK

TARGETLIBS=$(SDK_LIB_PATH)\user32.lib \
           $(SDK_LIB_PATH)\kernel32.lib

!IFNDEF DISABLE_NET_UNICODE
UNICODE=1
NET_C_DEFINES=-DUNICODE
!ENDIF

DLLBASE=0x1010000

USE_NTDLL=0

!IF $(FREEBUILD)
C_DEFINES=$(C_DEFINES)
!endif

INCLUDES=$(IFSKIT_INC_PATH); \
         $(DDK_INC_PATH); \
	 ..\sys	

SOURCES= dllmain.c \
         nulmrxnp.c

UMTYPE=console
OPTIONAL_NTTEST=

DLLDEF=$(_OBJ_DIR)\*\nulmrxnp.def

