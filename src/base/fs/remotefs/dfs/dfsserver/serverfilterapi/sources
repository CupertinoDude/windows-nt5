MINORCOMP=dfssrv

TARGETNAME=dfsFilterApi
TARGETPATH=$(_OBJ_DIR)
TARGETTYPE=LIBRARY

C_DEFINES=-DUNICODE

INCLUDES=.;..\..\inc;$(PROJECT_ROOT)\ntos\inc;
USE_MSVCRT=1

PASS0_HEADERDIR=$(O)
PASS0_SOURCEDIR=$(O)

MSC_WARNING_LEVEL=$(MSC_WARNING_LEVEL) /W3 /WX


RUN_WPP=$(SOURCES) -ini:..\..\inc\tracewpp.ini

SOURCES=\
    DfsFilterApi.cxx  

    

UMTYPE=console
