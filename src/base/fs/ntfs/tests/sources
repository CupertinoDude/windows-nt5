MAJORCOMP=cntfs
MINORCOMP=tests

TARGETNAME=ntfstest
TARGETPATH=obj
TARGETTYPE=LIBRARY

USE_MSVCRT=1

SOURCES=

UMTYPE=console
UMAPPL=quota*enumfile*opentest*rentest*bkup*findfile

UMLIBS=$(BASEDIR)\public\sdk\lib\*\ntdll.lib \
       $(BASEDIR)\public\sdk\lib\*\ole32.lib
	

C_DEFINES=$(C_DEFINES) -D_DCOM_
