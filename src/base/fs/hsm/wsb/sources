MAJORCOMP=hsm
MINORCOMP=Wsb

TARGETNAME=RsCommon
TARGETPATH=$(BASEP_HSM_DIR)\bin\$(_OBJ_DIR)
TARGETPATHLIB=$(BASEP_HSM_DIR)\lib\$(_OBJ_DIR)
TARGETTYPE=DYNLINK

# Force bringing in makefile.inc so better dependencies for RC file are known to build
NTTARGETFILES=

INCLUDES=$(BASEP_HSM_DIR)\idl\wsb\$(O);$(BASEP_HSM_DIR)\inc;$(BASEP_HSM_DIR)\inc\$(O);$(SDK_INC_PATH)

C_DEFINES=$(C_DEFINES) -D_MERGE_PROXYSTUB -D_UNICODE -DUNICODE -DWSB_IMPL


PASS0_SOURCEDIR=$(BASEP_HSM_DIR)\idl\wsb\$(O)
PASS0_HEADERDIR=$(BASEP_HSM_DIR)\inc\$(O)
TLBDIR=$(BASEP_HSM_DIR)\inc\$(O)

SOURCES= \
    dlldatax.c   \
    StdAfx.cpp   \
    Wsb.cpp      \
    wsbaccnt.cpp \
    wsbbstrg.cpp \
    wsbcltbl.cpp \
    wsbcltn.cpp  \
    wsbcore.cpp  \
    wsbenum.cpp  \
    wsbfile.cpp  \
    wsbguid.cpp  \
    wsbhash.cpp  \
    wsbport.cpp  \
    wsbpstbl.cpp \
    wsbpstrg.cpp \
    wsbregty.cpp \
    wsbserv.cpp  \
    wsbstrg.cpp  \
    wsbsvc.cpp   \
    wsbtrace.cpp \
    wsbtrak.cpp  \
    wsbtrc.cpp   \
    wsbusn.cpp   \
    wsbvar.cpp   \
    wsbfmt.cpp   \
    wsbvol.cpp   \
    wsb.rc       \



UMTYPE=windows
TARGETLIBS= \
    $(SDK_LIB_PATH)\advapi32.lib\
    $(SDK_LIB_PATH)\kernel32.lib\
    $(SDK_LIB_PATH)\netapi32.lib\
    $(SDK_LIB_PATH)\ole32.lib   \
    $(SDK_LIB_PATH)\rpcrt4.lib  \
    $(SDK_LIB_PATH)\rpcndr.lib  \
    $(SDK_LIB_PATH)\oleaut32.lib\
    $(SDK_LIB_PATH)\user32.lib  \
    $(SDK_LIB_PATH)\uuid.lib    \
    $(BASEP_HSM_DIR)\lib\$(O)\WsbGuid.lib \


# The following line should eventually be changed to a name in COFFBASE.TXT
DLLBASE=0x5f100000

DLLDEF=Wsb.def
DLLENTRY=_DllMainCRTStartup

USE_MSVCRT=1
