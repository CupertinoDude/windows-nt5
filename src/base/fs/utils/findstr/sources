USE_MSVCRT=1

W32_SB=1

TARGETNAME=findstr
TARGETPATH=obj
TARGETTYPE=PROGRAM

MSC_WARNING_LEVEL=/W3 /WX

SOURCES=qmatch.c   \
        recurse.c  \
        findstr.c  \
        findstr.rc \
        fsmsg.mc

UMTYPE=console

PASS0_HEADERDIR=$(O)
PASS0_SOURCEDIR=$(O)
INCLUDES=$(O)
MC_FLAGS=-v

USECXX_FLAG=/Tp

TARGETLIBS= \
    $(SDK_LIB_PATH)\binmode.obj \
    $(SDK_LIB_PATH)\user32.lib \
    $(SDK_LIB_PATH)\ntdll.lib
