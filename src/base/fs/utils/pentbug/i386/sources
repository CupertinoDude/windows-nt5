MAJORCOMP=utils
MINORCOMP=pentnt

TARGETNAME=pentnt
TARGETPATH=obj
TARGETTYPE=PROGRAM

INCLUDES=$(O)

PASS0_HEADERDIR=$(O)

i386_SOURCES=pentnt.c   \
             pentnt.rc  \
	     pbmsg.mc

UMTYPE=console
TARGETLIBS=\
    $(SDK_LIB_PATH)\user32.lib \
    $(SDK_LIB_PATH)\binmode.obj
