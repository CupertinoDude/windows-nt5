TARGETNAME=help
TARGETPATH=obj
TARGETTYPE=PROGRAM

UMTYPE=console
UMENTRY=wmain

USE_MSVCRT=1

C_DEFINES=-DUNICODE -D_UNICODE

INCLUDES=$(WINDOWS_INC_PATH)

PASS0_HEADERDIR=$(O)
PASS0_SOURCEDIR=$(O)

SOURCES=help.c      \
        help.rc     \
        msg.mc
        
TARGETLIBS=\
    $(SDK_LIB_PATH)\kernl32p.lib     


