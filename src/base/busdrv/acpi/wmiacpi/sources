MAJORCOMP=ntos
MINORCOMP=dd

TARGETNAME=wmiacpi
TARGETPATH=obj
TARGETTYPE=DRIVER

COMPILER_WARNINGS=-W3 -WX

SOURCES= \
        wmiacpi.rc \
        wmimap.c

WDMROOT=$(BASEDIR)\private\ntos\dd\wdm
INCLUDES= \
         $(WDMROOT)\acpi\inc;           \
         $(BASEDIR)\private\ntos\inc;
