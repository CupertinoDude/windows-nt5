USE_MSVCRT=1

TARGETNAME=tztool
TARGETPATH=$(_OBJ_DIR)
TARGETTYPE=PROGRAM

C_DEFINES= -DWIN32 -DNT $(DEBUG) -DWINVER=0x0400

SOURCES=tztool.rc   \
	tztool.c

UMENTRY=winmain

UMTYPE=windows

TARGETLIBS=$(SDK_LIB_PATH)\user32.lib \
           $(SDK_LIB_PATH)\kernel32.lib \
           $(SDK_LIB_PATH)\wmip.lib \
           $(SDK_LIB_PATH)\ntdll.lib \
           $(SDK_LIB_PATH)\comctl32.lib
