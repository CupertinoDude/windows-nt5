MAJORCOMP=windows
MINORCOMP=Wx86

TARGETNAME=sortpp
TARGETPATH=..
TARGETTYPE=PROGRAM
TARGETLIBS= ..\*\genlib.lib \
            $(SDK_LIB_PATH)\ntdll.lib

