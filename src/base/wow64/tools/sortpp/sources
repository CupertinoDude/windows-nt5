DLLENTRY=
SOURCES=sortpp.c \
        sortpp.rc

!IFNDEF MSC_WARNING_LEVEL
MSC_WARNING_LEVEL=/W3
!ENDIF
MSC_WARNING_LEVEL=$(MSC_WARNING_LEVEL) /WX

USE_MSVCRT=1

INCLUDES= .;..
UMTYPE=console
UMAPPL=
UMENTRY=main
