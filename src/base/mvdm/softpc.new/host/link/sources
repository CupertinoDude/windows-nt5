MINORCOMP=resources

TARGETNAME=resource


TARGETPATH=$(SOFTPC_DEST)\$(_OBJ_DIR)



TARGETTYPE=LIBRARY

INCLUDES=$(SOFTPC_ROOT)\HOST\INC

!IF $(ALPHA)
GPSIZE=0
!ELSE
GPSIZE=32
!ENDIF

SOURCES=resource.rc

UMTYPE=console
