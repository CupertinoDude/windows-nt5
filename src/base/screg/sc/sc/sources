TARGETNAME=sc
TARGETPATH=obj
TARGETTYPE=PROGRAM

USE_MSVCRT=1

MSC_WARNING_LEVEL=/W4 /WX

C_DEFINES= $(C_DEFINES) -DUNICODE -D_UNICODE

TARGETLIBS= \
    $(SDK_LIB_PATH)\user32.lib      \
    $(SDK_LIB_PATH)\advapi32.lib    \
    $(SDK_LIB_PATH)\ntdll.lib

INCLUDES=..\inc;

SOURCES= sc.c sc.rc

MISCFILES = \
     scdev.txt \
     sc_{CD275D74-EB38-4FF3-B8BE-E2F5E6E7100C}.sld

UMTYPE=console
UMENTRY=wmain
