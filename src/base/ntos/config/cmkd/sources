MAJORCOMP=ntos
MINORCOMP=cmkd

TARGETNAME=cmkd51
TARGETPATH=obj
TARGETTYPE=DYNLINK
TARGETLIBS=\nt\public\sdk\lib\*\kernel32.lib 

UMTYPE=windows
DLLDEF=cmkd.def

PRECOMPILED_INCLUDE=..\cmp.h
PRECOMPILED_PCH=cmp.pch
PRECOMPILED_OBJ=cmp.obj

INCLUDES=..\..\inc;..

SOURCES= cmkd.c \
		 cmkd.rc

UMTYPE=console

DLLENTRY=_DllMainCRTStartup
