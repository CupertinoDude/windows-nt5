MAJORCOMP=base
MINORCOMP=pnpidl

TARGETNAME =
TARGETPATH =
TARGETTYPE = NOTARGET

SYNCHRONIZE_BLOCK = 1

IDL_TYPE = RPC

PASS0_HEADERDIR = $(O)
PASS0_CLIENTDIR = $(O)
PASS0_SERVERDIR = $(O)

MIDL_FLAGS = $(MIDL_FLAGS) -oldnames

PASS0_PUBLISH=\
    {imports.h = ..\inc\imports.h}  \
    {$(PASS0_HEADERDIR)\pnp.h = ..\inc\pnp.h}

INCLUDES=\
    $(SDK_INC_PATH);                \
    $(CRT_INC_PATH);

SOURCES=\
    pnp.idl
