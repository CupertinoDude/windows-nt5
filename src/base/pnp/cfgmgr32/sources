MAJORCOMP=base
MINORCOMP=cfgmgr

TARGETNAME=cfgmgr
TARGETTYPE=LIBRARY
TARGETPATH=obj

PASS1_PUBLISH={$(O)\$(TARGETNAME).lib=$(SDK_LIB_PATH)\$(TARGETNAME).lib}

UMTYPE=windows

USE_MSVCRT=1
C_DEFINES=-DINCL_32= -DNT -DRPC_NO_WINDOWS_H -DUNICODE -D_CFGMGR32_
MSC_WARNING_LEVEL=/W3 /WX

PRECOMPILED_INCLUDE=precomp.h

INCLUDES=\
    ..\inc;                         \
    ..\lib;                         \
    ..\idl\$(O);                    \
    $(COM_INC_PATH);

