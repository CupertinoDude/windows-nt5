@c:*.xl?, %csidl_personal%
@c:*.csv, %csidl_personal%
@c:*.iqy, %csidl_personal%
@c:*.dqy, %csidl_personal%
@c:*.oqy, %csidl_personal%
@c:*.rqy, %csidl_personal%
@c:*.wk?, %csidl_personal%
