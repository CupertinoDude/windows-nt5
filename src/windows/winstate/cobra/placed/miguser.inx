@c:*.doc, %csidl_personal%
@c:*.dot, %csidl_personal%
@c:*.rtf, %csidl_personal%
@c:*.txt, %csidl_personal%\Text Files
@c:*.mcw, %csidl_personal%
@c:*.wps, %csidl_personal%
@c:*.scd, %csidl_personal%
@c:*.wri, %csidl_personal%
@c:*.wpd, %csidl_personal%
