@*: ----------- WsFTPLE 5.x -------------
@*:
[WsFTPLE5.Environment]
WsFTPLE5InstPath = IniFile, %windir%\win.ini/WS_FTP/DIR

[WsFTPLE5.Detect]
File, %WsFTPLE5InstPath%\WS_FTP95.EXE, VERSION("ProductVersion","6,*")

[WsFTPLE5.Instructions]
ForceSrcFile = WsFTPLE5.ForceSrcFile
CopyFilesEx = WsFTPLE5.CopyFilesEx

[WsFTPLE5.ForceSrcFile]
%WsFTPLE5InstPath%\WS_FTP.INI

[WsFTPLE5.CopyFilesEx]
%windir%\WS_FTP.CNV
%windir%\WS_FTP.EXT
%WsFTPLE5InstPath%\WS_FTP.INI, %WsFTPLE5InstPath%

