TARGETNAME=infcat
TARGETPATH=$(_OBJ_DIR)
TARGETTYPE=PROGRAM

USE_MFC=1

UMTYPE=console
UMENTRY=main

C_DEFINES= -DWIN32 -DW3

MSC_WARNING_LEVEL=/W3 /WX

PRECOMPILED_INCLUDE=stdafx.h
PRECOMPILED_CXX=1

SOURCES= \
         infcat.cpp  \
         mymfile.cpp \
         filemap.cpp \
         myinf.cpp   \
         mycmd.cpp   \
         cmd.cpp     \
         infcat.rc
