    // when this bit == 1, this char can be entered in composing char
    //  20-2F          /.-, +*)(  '&%$ #"!
        0x0000,     // 0000 0000  0000 0000

    //  30-3F          ?>=< ;:98  7654 3210
        0x03FF,     // 0000 0011  1111 1111

