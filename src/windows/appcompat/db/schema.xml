        <AttributeType name="VERFILEDATEHI"/>
        <AttributeType name="VERFILEOS"/>
        <AttributeType name="VERFILETYPE"/>
        <AttributeType name="PE_CHECKSUM"/>
        <AttributeType name="LINKER_VERSION"/>
