        <attribute type="SIZE"/>
        <attribute type="CHECKSUM"/>
        <attribute type="COMPANY_NAME"/>
        <attribute type="PRODUCT_NAME"/>
        <attribute type="PRODUCT_VERSION"/>
        <attribute type="FILE_DESCRIPTION"/>
