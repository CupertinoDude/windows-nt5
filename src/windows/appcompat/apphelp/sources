MAJORCOMP=apphelp
MINORCOMP=apphelp

TARGETNAME=apphelp
TARGETPATH=$(_OBJ_DIR)
TARGETTYPE=DYNLINK

BINPLACE_PLACEFILE=..\placefil.txt

USER_INCLUDES=$(WINDOWS_INC_PATH);$(SHELL_INC_PATH)


TARGETLIBS=    \
        $(SDK_LIB_PATH)\kernl32p.lib  \
        $(SDK_LIB_PATH)\shlwapi.lib   \
        $(SDK_LIB_PATH)\user32.lib    \
        $(SDK_LIB_PATH)\version.lib

LINKLIBS =     \
        $(PROJECT_ROOT)\appcompat\sdbAPI\lib\$(O)\sdbapil.lib

PASS1_PUBLISH=\
        {$(DYNLINK_LIB)=$(WINDOWS_LIB_DEST)\$(DYNLINK_LIBDIR)}

SOURCES= \
         match.c                \
         check.c                \
         persistLayers.c        \
         debug.c                \
         apphelp.rc


DLLENTRY=DllMain


DELAYLOAD = version.dll;shlwapi.dll;user32.dll

DLOAD_ERROR_HANDLER=shell32.dll

C_DEFINES= -DWIN32 -D_DEBUG_SPEW -DAPPHELP_TOOLS

USE_MSVCRT=0
USE_NTDLL=1

DLLDEF=apphelp.def

DLLBASE=0x1000000
UMTYPE=windows

PRECOMPILED_INCLUDE=apphelp.h

MSC_WARNING_LEVEL=/WX /W3
