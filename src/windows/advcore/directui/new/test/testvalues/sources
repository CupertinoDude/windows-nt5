MAJORCOMP=user

TARGETNAME=TestValues
TARGETPATH=$(_OBJ_DIR)
TARGETTYPE=PROGRAM
UMTYPE=console
UMENTRY=main
UMAPPL=TestValues

# Wait for the rest of DUI to build before we build our test.
