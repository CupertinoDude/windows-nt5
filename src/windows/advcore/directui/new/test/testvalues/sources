USE_MSVCRT=1

C_DEFINES=$(C_DEFINES) -DWIN32 -D_MBCS

PRECOMPILED_CXX=1
PRECOMPILED_INCLUDE=stdafx.h
PRECOMPILED_PCH=stdafx.pch
PRECOMPILED_OBJ=stdafx.obj

INCLUDES=\
        ..\..\Properties


SOURCES=\
        TestValues.cpp

TARGETLIBS=\
