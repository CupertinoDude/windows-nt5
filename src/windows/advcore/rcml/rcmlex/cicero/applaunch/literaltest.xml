<GRAMMAR>

	<RULE NAME="start/run/launch" TOPLEVEL="ACTIVE">
	    <L>
		<P>start</P>
		<P>run</P>
		<P>Launch</P>
	    </L>
	    <O>
		<RULEREF NAME="please"/>
	    </O>
	</RULE>

	<RULE NAME="email" TOPLEVEL="ACTIVE">
	    <L>
		<P>email</P>
		<P>send email to</P>
		<P>email to</P>
	    </L>

	    <DICTATION MIN="1" MAX="3"/>

	    <O>
		<RULEREF NAME="subject"/>
		<DICTATION MIN="1" MAX="3"/>
	    </O>

	    <O>
		<RULEREF NAME="please"/>
	    </O>

	</RULE>

	<RULE NAME="please" >
	    <L>
	    <P>please</P>
	    <P>thank you</P>
	    </L>
	</RULE>

	<RULE NAME="subject" >
	    <L>
		<P>subject</P>
		<P>about</P>
		<P>with the subject</P>
	    </L>
	</RULE>

</GRAMMAR>