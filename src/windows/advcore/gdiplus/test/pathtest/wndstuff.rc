        MENUITEM "&Print...",                   MM_PRINT
    END
    POPUP "Path"
    BEGIN
        POPUP "&Mode"
        BEGIN
            MENUITEM "&Lines\tl",                   MM_LINEMODE, CHECKED
            MENUITEM "&Bezier\tb",                  MM_BEZIERMODE
            MENUITEM SEPARATOR
            MENUITEM "Outline Before",              MM_OBEFOREMODE
            MENUITEM "Outline After",               MM_OAFTERMODE, CHECKED
        END
        POPUP "Flattness"
        BEGIN
            MENUITEM "10.0",                        MM_FLATNESS_1
            MENUITEM "1.0",                         MM_FLATNESS_2
            MENUITEM "0.25 (Default)",              MM_FLATNESS_3, CHECKED
            MENUITEM "0.1",                         MM_FLATNESS_4
        END
        MENUITEM "&Close path\tc",              MM_CLOSEPATH
        MENUITEM "C&lear path\te",              MM_CLEARPATH
        MENUITEM "Use as clip path",            MM_USEASCLIPPATH
        MENUITEM "Clear clip path",             MM_CLEARCLIPPATH
    END
    POPUP "Fill"
    BEGIN
        MENUITEM "Transparent solid fill (alt)\tf", MM_SHOWTRANSSOLIDFILL
        MENUITEM "Transparent gradient fill (alt)\tg", MM_SHOWTRANSGRADFILL
        POPUP "Texture"
        BEGIN
            MENUITEM "Texture fill (wind)\tt",      MM_SHOWTEXTUREFILL
            , CHECKED
            MENUITEM "Change texture...",           MM_CHANGETEXTUREFILL
            POPUP "Color mode"
            BEGIN
                MENUITEM "No change",                   MM_COLORMODE_NOCHANGE
                , CHECKED
                MENUITEM "50% transparent",             MM_COLORMODE_TRANS50
                MENUITEM "Gradient transparency",       MM_COLORMODE_GRADTRANS

            END
        END
        MENUITEM "Background Gradient\tb",      MM_SHOWBGGRADFILL
    END
    POPUP "Stroke"
    BEGIN
        MENUITEM "Complex dash",                MM_SHOWDASHSTROKE
        MENUITEM "Hatch brush",                 MM_SHOWHATCHBRUSHSTROKE
    END
END


