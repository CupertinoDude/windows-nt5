MSC_WARNING_LEVEL=/W4 /WX

GDIPROOT=$(ADVCORE_PATH)\gdiplus

MAJORCOMP=windows
MINORCOMP=crop

TARGETNAME=crop
TARGETPATH=$(_OBJ_DIR)
TARGETTYPE=PROGRAM

USE_MSVCRT=1
C_DEFINES=-DUNICODE -D_UNICODE

C_DEFINES=$(C_DEFINES)


INCLUDES=$(GDIPROOT)\sdkinc

SOURCES=crop.cxx 

UMTYPE=console
UMLIBS=$(GDIPROOT)\engine\flat\dll\$(O)\gdiplus.lib
       
