MAJORCOMP=windows
MINORCOMP=dciperf

TARGETNAME=dciperf
TARGETPATH=$(_OBJ_DIR)
TARGETTYPE=PROGRAM
TARGETLIBS=


SOURCES=wndstuff.c \
        dciperf.c     \
        wndstuff.rc

UMTYPE=console
UMAPPL=dciperf

UMLIBS=$(SDK_LIB_PATH)\user32.lib \
       $(SDK_LIB_PATH)\kernel32.lib \
       $(SDK_LIB_PATH)\gdi32.lib \
       $(WINDOWS_LIB_PATH)\dciman32.lib \
       $(SDK_LIB_PATH)\ddraw.lib 
