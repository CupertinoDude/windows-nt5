        <ID NAME="Keyboard_ID_Page_Up"          VAL="44"/>
        <ID NAME="Keyboard_ID_Page_Down"        VAL="45"/>
        <ID NAME="Keyboard_ID_Tab"              VAL="46"/>
        <ID NAME="Keyboard_ID_Enter"            VAL="47"/>
        <ID NAME="Keyboard_ID_Backspace"        VAL="48"/>
