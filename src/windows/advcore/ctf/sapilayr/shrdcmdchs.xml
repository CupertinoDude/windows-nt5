        <ID NAME="Select_ID_SELPARAGRAPH"       VAL="12"/>
        <ID NAME="Select_ID_SELWORD"            VAL="13"/>
        <ID NAME="Select_ID_SelectAll"          VAL="14"/>
        <ID NAME="Select_ID_DeletePhrase"       VAL="15"/>
        <ID NAME="Select_ID_Convert"            VAL="16"/>
