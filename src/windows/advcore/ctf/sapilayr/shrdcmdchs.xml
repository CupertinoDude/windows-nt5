<GRAMMAR LANGID="804">
    <DEFINE>

        <ID NAME="Select_ID_SELECT"             VAL="1"/>
        <ID NAME="Select_ID_DELETE"             VAL="2"/>
        <ID NAME="Select_ID_CORRECT"            VAL="3"/>
        <ID NAME="Select_ID_SELTHROUGH"         VAL="4"/>
        <ID NAME="Select_ID_DELTHROUGH"         VAL="5"/>
