W32_SB=1

TARGETNAME=wow64lg2
MAJORCOMP=wow64
MINORCOMP=wow64lg2
TARGETTYPE=DYNLINK
TARGETPATH=$(_OBJ_DIR)

INCLUDES=$(INCLUDES);..\cgen\$(O);..\cgen;$(O);$(BASE_INC_PATH)

USE_NTDLL=1


!IFNDEF MSC_WARNING_LEVEL
MSC_WARNING_LEVEL=/W3
!ENDIF
MSC_WARNING_LEVEL=$(MSC_WARNING_LEVEL) /WX

SOURCES=$(O)\whcon.c \
        $(O)\whwin32.c \
        wow64lg2.rc

DLLDEF=wow64lg2.def

UMTYPE=windows
