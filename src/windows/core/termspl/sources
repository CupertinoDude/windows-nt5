MAJORCOMP=user
MINORCOMP=server

TARGETNAME=termspl
TARGETPATH=$(_OBJ_DIR)
TARGETTYPE=LIBRARY

INCLUDES=$(PRINTSCAN_INC_PATH)\winsrv;	\
         $(WINCORE_PATH)\w32inc         \

MSC_WARNING_LEVEL=/W3 /WX

SOURCES=            \
        kmspool.c   \
        musspl.c    \
        splkernl.c  \
        yspool.c    \
	umpdhook.c

C_DEFINES=$(C_DEFINES) -DUNICODE

PRECOMPILED_INCLUDE=precomp.h
PRECOMPILED_PCH=precomp.pch
PRECOMPILED_OBJ=precomp.obj
