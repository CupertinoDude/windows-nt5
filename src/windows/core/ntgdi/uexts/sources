MAJORCOMP=windows
MINORCOMP=gdiexts

TARGETNAME=gdiexts
TARGETPATH=$(_OBJ_DIR)
TARGETTYPE=DYNLINK
UMTYPE=console

INCLUDES=..\client;                         \
         ..\inc;                            \
         $(WINCORE_PATH)\w32inc;            \
         ..\direct\ddraw\main;              \


#USE_MSVCRT=1

LINKLIBS=  $(SDK_LIB_PATH)\kernel32.lib     \
           $(NTGDI_PATH)\math\$(ALT_PROJECT_TARGET)\$(O)\efloat.lib



NTPROFILEINPUT=yes

#
# Setup for application developer to set breakpoints with windbg
#

NTDEBUGTYPE=coff

SOURCES=debug.c      \
        gdiexts.rc
