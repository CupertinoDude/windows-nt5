MAJORCOMP=windows
MINORCOMP=GDI

TARGETNAME=dciman32
TARGETTYPE=DYNLINK

TARGETPATH=$(_OBJ_DIR)
PASS1_PUBLISH=\
    {$(DYNLINK_LIB)=$(WINDOWS_LIB_DEST)\$(DYNLINK_LIBDIR)}

TARGETLIBS=$(WINDOWS_LIB_PATH)\gdi32p.lib    \
           $(SDK_LIB_PATH)\user32.lib    \
           $(SDK_LIB_PATH)\kernel32.lib


DLLENTRY=DciDllInitialize

!IFNDEF MSC_WARNING_LEVEL
MSC_WARNING_LEVEL=/W3
!ENDIF
MSC_WARNING_LEVEL=$(MSC_WARNING_LEVEL) /WX

INCLUDES=..\inc;                        \
         ..\client;                     \
         ..\direct\ddraw\main;          \
         $(MULTIMEDIA_INC_PATH);        \
         $(DDK_INC_PATH)


SOURCES= dllinit.c      \
         dciman.c       \
         dciman.rc
