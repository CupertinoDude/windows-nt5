        MENUITEM "&Delete",             CMIDM_DELETE
        MENUITEM "Rena&me",             CMIDM_RENAME
        MENUITEM "", -1, MFT_SEPARATOR
        MENUITEM "P&roperties",         CMIDM_PROPERTIES
    END
END


