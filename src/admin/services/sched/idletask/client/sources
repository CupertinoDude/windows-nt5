MAJORCOMP=scheduler
MINORCOMP=idletask

TARGETNAME=idletskc
TARGETTYPE=LIBRARY
TARGETPATH=obj

C_DEFINES=                                        \
    $(C_DEFINES)                                  \
    -DUNICODE                                     \
    -D_UNICODE                                    \

INCLUDES=                                         \
    ..\inc;                                       \
    ..\idl\$(O); \
    $(O);                                         \
    $(INCLUDES)                                   \

SOURCES=                                          \
    idletskc.c                                    \
    idlrpc_client.c

PASS1_PUBLISH=\
    {$(O)\idletskc.lib=$(PROJECT_LIB_PATH)\idletskc.lib}