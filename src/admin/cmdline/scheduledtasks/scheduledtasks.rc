    IDS_CREATE_HLP73        "    DAILY:   1 - 365 days.\n"
    IDS_CREATE_HLP74        "    WEEKLY:  weeks 1 - 52.\n"
    IDS_CREATE_HLP75        "    ONCE:    No modifiers.\n"
    IDS_CREATE_HLP76        "    ONSTART: No modifiers.\n"
    IDS_CREATE_HLP77        "    ONLOGON: No modifiers.\n"
    IDS_CREATE_HLP78        "    ONIDLE:  No modifiers.\n"
