STRINGTABLE DISCARDABLE 
BEGIN
    IDS_CREATE_HLP1         "\nSCHTASKS /Create [/S system [/U username [/P password]]]\n"                
    IDS_CREATE_HLP2         "    [/RU username [/RP password]] /SC schedule [/MO modifier] [/D day]\n"
    IDS_CREATE_HLP3         "    [/I idletime] /TN taskname /TR taskrun [/ST starttime] [/M months]\n"
    IDS_CREATE_HLP4         "    [/SD startdate] [/ED enddate]\n\n"
	IDS_CREATE_HLP5         "Description:\n "
    IDS_CREATE_HLP6         "   Enables an administrator to create scheduled tasks on a local or\n "
    IDS_CREATE_HLP7         "   remote systems. \n\n"
    IDS_CREATE_HLP8         "Parameter List:\n"
    IDS_CREATE_HLP9         "    /S           system            Specifies the remote system to\n"
    IDS_CREATE_HLP10        "                                   connect to. If omitted the system\n"
    IDS_CREATE_HLP11        "                                   parameter defaults to the local\n"
	IDS_CREATE_HLP12        "                                   system.\n\n"
	IDS_CREATE_HLP13        "    /U           username          Specifies the user context under\n"
    IDS_CREATE_HLP14        "                                   which the command should execute.\n\n"
    IDS_CREATE_HLP15        "    /P           password          Specifies the password for the given\n"
    IDS_CREATE_HLP16        "                                   user context.\n\n"
    IDS_CREATE_HLP17        "    /RU          username          Specifies the user account (user \n"
    IDS_CREATE_HLP18        "                                   context) under which the task runs.\n"
	IDS_CREATE_HLP19        "                                   For the system account, valid values \n"
	IDS_CREATE_HLP20        "                                   are """", ""NT AUTHORITY\SYSTEM"" or\n"
	IDS_CREATE_HLP21        "                                   ""SYSTEM"".\n\n"
    IDS_CREATE_HLP22        "    /RP          password          Specifies the password for the user.\n"
    IDS_CREATE_HLP23        "                                   To prompt for the password, the value\n"
    IDS_CREATE_HLP24        "                                   must be either ""*"" or none.\n"
	IDS_CREATE_HLP25        "                                   Password will not effect for the\n"
	IDS_CREATE_HLP26        "                                   system account.\n\n"
	IDS_CREATE_HLP27        "    /SC          schedule          Specifies the schedule frequency.\n"
    IDS_CREATE_HLP28        ""
    IDS_CREATE_HLP29        "                                   Valid schedule types: MINUTE, HOURLY,\n"
    IDS_CREATE_HLP30        "                                   DAILY, WEEKLY, MONTHLY, ONCE,\n"
    IDS_CREATE_HLP31        "                                   ONSTART, ONLOGON, ONIDLE.\n\n"
    IDS_CREATE_HLP32        ""
    IDS_CREATE_HLP33        "    /MO          modifier          Refines the schedule type to allow \n"
    IDS_CREATE_HLP34        "                                   finer control over schedule\n"
    IDS_CREATE_HLP35        "                                   recurrence. Valid values are listed \n"
    IDS_CREATE_HLP36        "                                   in the ""Modifiers"" section below.\n\n"
    IDS_CREATE_HLP37        "    /D           days              Specifies the day of the week to run\n"
    IDS_CREATE_HLP38        "                                   the task. Valid values: MON, TUE,\n"
    IDS_CREATE_HLP39        "                                   WED, THU, FRI, SAT, SUN and for\n"
    IDS_CREATE_HLP40        "                                   MONTHLY schedules 1 - 31 (days of the\n"
	IDS_CREATE_HLP41        "                                   month).\n\n"
    IDS_CREATE_HLP42        "    /M           months            Specifies month(s) of the year.\n"
    IDS_CREATE_HLP43        "                                   Defaults to the first day of the\n"
    IDS_CREATE_HLP44        "                                   month. Valid values: JAN, FEB, MAR,\n"
    IDS_CREATE_HLP45        "                                   APR, MAY, JUN, JUL, AUG, SEP, OCT,\n"
    IDS_CREATE_HLP46        "                                   NOV, DEC.\n\n"
    IDS_CREATE_HLP47        "    /I           idletime          Specifies the amount of idle time to \n"
    IDS_CREATE_HLP48        "                                   wait before running a scheduled \n"
    IDS_CREATE_HLP49        "                                   ONIDLE task.\n"
	IDS_CREATE_HLP50        "                                   Valid range: 1 - 999 minutes.\n\n"
