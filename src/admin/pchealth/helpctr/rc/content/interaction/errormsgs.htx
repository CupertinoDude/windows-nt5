#endif
        </td>
		<td id="TD2TR1idMsgTbl" name="TD2TR1idMsgTbl" class="sys-font-body sys-color-body padding6 padding8">  
			<Span id="MessageId" name="MessageId"> &nbsp; </Span>
		</td>	
	</tr>
	<tr>
