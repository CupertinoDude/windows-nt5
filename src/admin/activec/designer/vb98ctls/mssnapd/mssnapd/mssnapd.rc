BEGIN
    POPUP "Root"
    BEGIN
        MENUITEM "Rename",                  CMD_RENAME, GRAYED
        MENUITEM "Properties",              CMD_VIEW_PROPERTIES, GRAYED
        MENUITEM "Convert Dialog Units...", CMD_VIEW_DLGUNITS
    END
END

// StaticNode Menu
IDR_MENU_STATIC_NODE MENU DISCARDABLE
BEGIN
    POPUP "StaticNode"
    BEGIN
        MENUITEM "Add child node",        CMD_ADD_NODE, GRAYED
        POPUP "Add New Result View"
        BEGIN
            MENUITEM "List view",         CMD_ADD_LISTVIEW, GRAYED
            MENUITEM "OCX view",          CMD_ADD_OCX_VIEW, GRAYED
            MENUITEM "URL view",          CMD_ADD_WEB_VIEW, GRAYED
            MENUITEM "Taskpad",           CMD_ADD_TASKPAD, GRAYED
        END
        POPUP "Add Existing Result View"
        BEGIN
            MENUITEM ""                   0, GRAYED
        END
        MENUITEM "Properties",            CMD_VIEW_PROPERTIES, GRAYED
    END
END

