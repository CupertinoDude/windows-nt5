TARGETNAME= dsadminp
TARGETTYPE= LIBRARY
TARGETPATH= obj

PRECOMPILED_CXX=1
PRECOMPILED_INCLUDE=pch.h

MSC_WARNING_LEVEL= /W4
USE_MFCUNICODE=1
USE_STD=1
USE_ATL=1
ATL_VER=30
USE_MSVCRT=1


C_DEFINES   = $(C_DEFINES) -DSTRICT -DUNICODE -D_UNICODE

TARGETLIBS=\
    $(SDK_LIB_PATH)\kernel32.lib \
    $(SDK_LIB_PATH)\user32.lib \
    $(SDK_LIB_PATH)\libc.lib \

INCLUDES=\
    ..\inc;              \
    $(ADMIN_INC_PATH);   \
    $(DS_INC_PATH);       

SOURCES= \
    _dsadmin.cpp      \
    basePathsInfo.cpp 


