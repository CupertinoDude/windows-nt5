MAJORCOMP=ntos
MINORCOMP=dd

NTDEBUG=ntsd
NTDEBUGTYPE=both
MSC_OPTIMIZATION=/Odi

TARGETNAME=sample
TARGETPATH=obj
TARGETTYPE=PROGRAM

INCLUDES=d:\nt\private\ntos\inc

SOURCES=testsample.c 

UMENTRY=main
UMTYPE=console

TARGETLIBS=$(BASEDIR)\public\sdk\lib\*\ntdll.lib
NTDEBUG=ntsd 
MSC_OPTIMIZATION=/Odi