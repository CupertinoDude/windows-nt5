!ifndef FAXROOT
FAXROOT=$(PROJECT_ROOT)\fax
!endif

!include $(FAXROOT)\faxsrc.inc

TARGETNAME=faxshell
TARGETPATH=obj
TARGETTYPE=DYNLINK

DLLDEF=$(O)\faxshell.def
DLLENTRY=DllMain

