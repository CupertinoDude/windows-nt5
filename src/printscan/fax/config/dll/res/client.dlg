    LTEXT           "&Company:",IDCSTATIC_COMPANY,134,107,35,10
    EDITTEXT        IDC_SENDER_COMPANY,178,106,66,13,ES_AUTOHSCROLL |
                    WS_GROUP
    LTEXT           "&Office location:",IDCSTATIC_OFFICE,8,123,50,8
