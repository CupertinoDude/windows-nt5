IDD_CLIENT_COVERPG DIALOG DISCARDABLE  0, 0, 252, 218
STYLE WS_CHILD | WS_VISIBLE | WS_CAPTION
CAPTION "Cover Page"
FONT 8, "MS Shell Dlg"
BEGIN
    ICON            IDI_COVERPG,IDCSTATIC_COVERPAGE_ICON,8,6,18,20
    LTEXT           "Personal Cover Pages",IDCSTATIC_COVER_PAGE,36,8,208,16
    LISTBOX         IDC_COVERPG_LIST,8,30,236,88,LBS_SORT |
                    LBS_NOINTEGRALHEIGHT | WS_VSCROLL | WS_GROUP |
                    WS_TABSTOP
    PUSHBUTTON      "&Open",IDC_COVERPG_OPEN,8,123,55,14
    PUSHBUTTON      "&New",IDC_COVERPG_NEW,68,123,55,14
    PUSHBUTTON      "A&dd",IDC_COVERPG_ADD,128,123,55,14
    PUSHBUTTON      "D&elete",IDC_COVERPG_REMOVE,188,123,55,14
END

IDD_USER_INFO DIALOGEX 0, 0, 252, 218
STYLE WS_CHILD | WS_VISIBLE | WS_CAPTION
