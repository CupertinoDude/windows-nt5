  $(SDK_LIB_PATH)\comctl32.lib \
  $(SDK_LIB_PATH)\user32.lib   \
  $(SDK_LIB_PATH)\advapi32.lib \
  $(SDK_LIB_PATH)\comctl32.lib \
  $(SDK_LIB_PATH)\tapi32.lib   \
  $(SDK_LIB_PATH)\winspool.lib \
  $(SDK_LIB_PATH)\mpr.lib      \
  $(SDK_LIB_PATH)\uuid.lib

INCLUDES=\
  $(INCLUDES); \
  $(FAXROOT)\tiff\inc; \
  $(FAXROOT)\tiff\src

