!ifndef FAXROOT
FAXROOT=$(PROJECT_ROOT)\fax
!endif

!include $(FAXROOT)\faxsrc.inc

TARGETNAME=faxsvc
TARGETPATH=obj
TARGETTYPE=PROGRAM

UMTYPE=console
LINKER_STACKSIZE=-stack:1048576,4096
!if $(FREEBUILD)
DELAYLOAD=advapi32.dll;ole32.dll;tapi32.dll;faxmapi.dll
!else
DELAYLOAD=imagehlp.dll;version.dll;advapi32.dll;ole32.dll;tapi32.dll;faxmapi.dll
!endif

!ifdef NOUNICODE
UMENTRY=winmain
!else
UMENTRY=wwinmain
!endif

BACKGROUND_USE=1
NTKEEPRESOURCETMPFILES=1
USE_MSVCRT=1

PRECOMPILED_INCLUDE=faxsvc.h
PRECOMPILED_PCH=precomp.pch
PRECOMPILED_OBJ=precomp.obj

C_DEFINES=$(C_DEFINES) -DFAX_WORKSTATION_ONLY=0

INCLUDES=\
  ..\registry; \
  $(INCLUDES);\
  ..\inc;\
  $(FAXROOT)\tiff\inc;\
  $(FAXROOT)\tiff\src;\
  $(BASEDIR)\private\windows\inc;\
  ..\faxevent

TARGETLIBS=\
  $(SDK_LIB_PATH)\rpcrt4.lib                \
  $(SDK_LIB_PATH)\rpcndr.lib                \
  $(SDK_LIB_PATH)\rpcutil.lib               \
  $(SDK_LIB_PATH)\tapi32.lib                \
  $(SDK_LIB_PATH)\ntdll.lib                 \
  $(SDK_LIB_PATH)\uuid.lib                  \
  $(SDK_LIB_PATH)\mpr.lib                   \
  $(SDK_LIB_PATH)\gdi32.lib                 \
  $(SDK_LIB_PATH)\winspool.lib              \
  $(SDK_LIB_PATH)\user32.lib                \
!if !$(FREEBUILD)
  $(SDK_LIB_PATH)\imagehlp.lib              \
  $(SDK_LIB_PATH)\version.lib               \
!endif
  $(SDK_LIB_PATH)\ole32.lib                 \
  $(SDK_LIB_PATH)\wsock32.lib               \
  $(SDK_LIB_PATH)\uuid.lib                  \
  $(SDK_LIB_PATH)\shell32.lib               \
  $(FAXROOT)\service\registry\$O\faxreg.lib             \
  $(FAXROOT)\service\faxevent\$O\faxevent.lib           \
  $(FAXROOT)\service\rpc\stubs\faxsvc\$O\svcstub.lib    \
  $(FAXLIB)\*\faxmapi.lib                               \
  $(FAXLIB)\*\faxtiff.lib

SOURCES=\
  faxdev.c    \
  faxrpc.c    \
  faxsvc.c    \
  faxsvc.rc   \
  faxsvmsg.mc \
  handle.c    \
  job.c       \
  queue.c     \
  receive.c   \
  route.c     \
  security.c  \
  server.c    \
  tapi.c      \
  tapidbg.c   \
  util.c
