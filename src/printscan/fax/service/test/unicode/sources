  $(LINKLIBS) \
  $(SDK_LIB_PATH)\user32.lib \
  $(SDK_LIB_PATH)\gdi32.lib \
  $(SDK_LIB_PATH)\shell32.lib \
  $(FAXLIB)\*\faxtiffp.lib \
  $(SDK_LIB_PATH)\winfax.lib

SOURCES=\
  ..\faxtest.c \
  ..\faxtest.rc
