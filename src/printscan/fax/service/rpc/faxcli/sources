!ifndef FAXROOT
FAXROOT=$(PROJECT_ROOT)\fax
!endif

!include $(FAXROOT)\faxsrc.inc

TARGETPATH=obj
TARGETTYPE=NOTARGET

IDL_TYPE=RPC

PASS0_CLIENTDIR=$(FAXROOT)\service\rpc\stubs\faxsvc
PASS0_SERVERDIR=$(FAXROOT)\service\rpc\stubs\faxcli
PASS0_HEADERDIR=$(FAXROOT)\service\inc

MIDL_OPTIMIZATION=$(MIDL_OPTIMIZATION_NT4) -error all


MIDL_FLAGS=$(MIDL_FLAGS) -oldnames -out $(PASS0_HEADERDIR) -I$(PASS0_HEADERDIR)

SOURCES=faxcli.idl

SYNCHRONIZE_BLOCK=1
