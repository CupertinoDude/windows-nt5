MAJORCOMP=spooler
MINORCOMP=perf

TARGETNAME=perf
TARGETPATH=obj
TARGETTYPE=LIBRARY
TARGETLIBS=

INCLUDES=.;..\idl;..;..\..\inc;$(WINDOWS_INC_PATH)

