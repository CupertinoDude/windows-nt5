#
# The TARGETNAME variable is defined by the developer.  It is the name of
# the target (component) that is being built by this makefile.  It
# should NOT include any path or file extension information.
#

MAJORCOMP =windows
MINORCOMP =spoolsv


LINKLIBS=$(PROJECT_ROOT)\lib\*\spllib.lib

TARGETNAME=splwow64
TARGETPATH=obj
#
# The TargetType PROGLIB allows the binary to be an executable as well as
# a library which exports functions
#
TARGETTYPE=PROGRAM

BACKGROUND_USE=1

INCLUDES=.;..;..\idl;          \
         ..\..\..\inc;         \
         ..\..\client;         \
         $(WINDOWS_INC_PATH);  \
         $(PROJECT_INC_PATH);

USE_MSVCRT=1

SOURCES=    ldmgr.cxx     \
            ldmemmgr.cxx  \
            lddevcap.cxx  \
            splwow64s.c   \
            ldmain.cxx    \
            ldfuncs.cxx   \
            lderror.cxx   \
            drvevnt.cxx   \
            prtcfg.cxx    \
            basecls.cxx   \
            ldintrfcs.cxx \
            prntuifn.cxx  \
            gdithnk.cxx   \
            lpcmgr.cxx    \
            splwow64.rc
            

C_DEFINES= -DNTOS2 -Dfree=_delete -DRPC_NO_WINDOWS_H -DUNICODE -DNO_STRICT -DDBGLOG -DSPOOLKM

UMTYPE=windows

TARGETLIBS= \
    $(SDK_LIB_PATH)\winspool.lib \
    $(PROJECT_ROOT)\lib\*\spoolss.lib \
    $(SDK_LIB_PATH)\rpcrt4.lib  \
    $(SDK_LIB_PATH)\rpcndr.lib  \
    $(SDK_LIB_PATH)\uuid.lib  \
    $(SDK_LIB_PATH)\ntdll.lib
    
    
