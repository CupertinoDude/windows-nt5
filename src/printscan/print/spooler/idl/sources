TARGETTYPE=LIBRARY
TARGETPATH=$(PROJECT_ROOT)\lib
TARGETNAME=spluuid

PASS0_PUBLISH=\
              {$(O)\bidispl.h=$(SDK_INC_PATH)\bidispl.h}

PASS1_PUBLISH=\
              {$(TARGETPATH)\$(TARGET_DIRECTORY)\$(TARGETNAME).lib=$(PRINTSCAN_LIB_PATH)\spluuid.lib}
MIDL_UUIDDIR=.
PASS0_SERVERDIR=$(O)
PASS0_CLIENTDIR=$(O)
PASS0_HEADERDIR=$(O)

SOURCES=\
    bidispl.idl \
    $(O)\bidispl_i.c \
    bidisplp.idl \
    $(O)\bidisplp_i.c


