DataFile=PDLBP400K.GPD

[SSF500PK.GPD]
CopyFiles=@PCL5KRES.DLL,@SSF500PK.GPD,UNIDRV
DataSection=UNIDRV_DATA
DataFile=SSF500PK.GPD

[SSF800PK.GPD]
CopyFiles=@PCL5KRES.DLL,@SSF800PK.GPD,UNIDRV
