DataFile=SSML68HK.GPD

[SR3910RK.GPD]
CopyFiles=@PCL5KRES.DLL,@SR3910RK.GPD,UNIDRV
DataSection=UNIDRV_DATA
DataFile=SR3910RK.GPD

[SR3912RK.GPD]
CopyFiles=@PCL5KRES.DLL,@SR3912RK.GPD,UNIDRV
