OEMINC=..\..\oeminc

MAJORCOMP=windows
MINORCOMP=md_casn4res

TARGETNAME=casn4res
TARGETPATH=$(BASEDIR)\lib
TARGETTYPE=DYNLINK
!IF 0
TARGETLIBS=
!ELSE
TARGETLIBS=$(BASEDIR)\lib\*\$(DDKBUILDENV)\libcntpr.lib \
           $(DDK_LIB_PATH)\$(DDKBUILDENV)\win32k.lib
