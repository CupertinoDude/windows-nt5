DLLBASE=0x8000000

INCLUDES=..\..\inc;$(OEMINC)

C_DEFINES=$(C_DEFINES) -DKERNEL_MODE -DWINNT_40

RCCODEPAGE=932

NO_NTDLL=1

SOURCES=casn4res.rc \
	casn4res.c \
	common.c \
	debug.c

