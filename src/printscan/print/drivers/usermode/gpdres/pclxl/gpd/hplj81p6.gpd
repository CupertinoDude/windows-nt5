*switch: Collate
{
    *case: OFF
    {
        *Command: CmdEndPage
        {
            *Order: PAGE_FINISH.1
            *Cmd: =uint16 =NumOfCopy =attr_ubyte =PageCopies =EndPage
        }
    }
    *case: ON
    {
        *Command: CmdEndPage
        {
            *Order: PAGE_FINISH.1
            *Cmd: =uint16 %l{1} =attr_ubyte =PageCopies =EndPage
        }
    }
}

