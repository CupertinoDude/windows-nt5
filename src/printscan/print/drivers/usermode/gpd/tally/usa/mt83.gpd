    {
        *rcNameID: =RCID_DMPAPER_SYSTEM_NAME
        *Command: CmdSelect
        {
            *Order: DOC_SETUP.8
            *Cmd: "<1B>2<1B>CF"
        }
        *switch: InputBin
        {
            *case: TRACTOR
            {
                *PrintableArea: PAIR(2880, 4208)
                *PrintableOrigin: PAIR(90, 0)
            }
            *case: MANUAL
            {
                *PrintableArea: PAIR(2880, 4116)
                *PrintableOrigin: PAIR(90, 68)
            }
            *case: AUTO
            {
                *PrintableArea: PAIR(2880, 4028)
                *PrintableOrigin: PAIR(90, 180)
            }
            *case: Option4
            {
                *PrintableArea: PAIR(2880, 4208)
                *PrintableOrigin: PAIR(90, 0)
            }
        }
    }
    *Option: LETTER
    {
        *rcNameID: =RCID_DMPAPER_SYSTEM_NAME
        *Command: CmdSelect
        {
            *Order: DOC_SETUP.8
            *Cmd: "<1B>C<000B>"
        }
        *switch: InputBin
        {
            *case: TRACTOR
            {
                *PrintableArea: PAIR(2880, 3960)
                *PrintableOrigin: PAIR(90, 0)
            }
            *case: MANUAL
            {
                *PrintableArea: PAIR(2880, 3868)
                *PrintableOrigin: PAIR(90, 68)
            }
            *case: AUTO
            {
                *PrintableArea: PAIR(2880, 3780)
                *PrintableOrigin: PAIR(90, 180)
            }
            *case: Option4
            {
                *PrintableArea: PAIR(2880, 3960)
                *PrintableOrigin: PAIR(90, 0)
            }
        }
    }
    *Option: A5
    {
        *rcNameID: =RCID_DMPAPER_SYSTEM_NAME
        *Command: CmdSelect
        {
            *Order: DOC_SETUP.8
            *Cmd: "<1B>2<1B>C2"
        }
        *switch: InputBin
        {
            *case: TRACTOR
            {
                *PrintableArea: PAIR(2009, 2977)
                *PrintableOrigin: PAIR(90, 0)
                *switch: Resolution
                {
                    *case: Option1
                    {
                        *PrintableArea: PAIR(2008, 2976)
                    }
                    *case: Option2
                    {
                        *PrintableArea: PAIR(2009, 2976)
                    }
                    *case: Option3
                    {
                        *PrintableArea: PAIR(2007, 2976)
                    }
                }
            }
            *case: MANUAL
            {
                *PrintableArea: PAIR(2009, 2885)
                *PrintableOrigin: PAIR(90, 68)
                *switch: Resolution
                {
                    *case: Option1
                    {
                        *PrintableArea: PAIR(2008, 2884)
                    }
                    *case: Option2
                    {
                        *PrintableArea: PAIR(2009, 2884)
                    }
                    *case: Option3
                    {
                        *PrintableArea: PAIR(2007, 2884)
                    }
                }
            }
            *case: AUTO
            {
                *PrintableArea: PAIR(2009, 2797)
                *PrintableOrigin: PAIR(90, 180)
                *switch: Resolution
                {
                    *case: Option1
                    {
                        *PrintableArea: PAIR(2008, 2796)
                    }
                    *case: Option2
                    {
                        *PrintableArea: PAIR(2009, 2796)
                    }
                    *case: Option3
                    {
                        *PrintableArea: PAIR(2007, 2796)
                    }
                }
            }
            *case: Option4
            {
                *PrintableArea: PAIR(2009, 2977)
                *PrintableOrigin: PAIR(90, 0)
                *switch: Resolution
                {
                    *case: Option1
                    {
                        *PrintableArea: PAIR(2008, 2976)
                    }
                    *case: Option2
                    {
                        *PrintableArea: PAIR(2009, 2976)
                    }
                    *case: Option3
                    {
                        *PrintableArea: PAIR(2007, 2976)
                    }
                }
            }
        }
    }
