    PS_POSTCARD           : 45
    PS_FREE               : 48

    CBID_PORT             : 50
    CBID_LAND             : 51

    PRN_2000              : 60

    OPT_NOSET             : Option1

    OPT_B4                : Option3
    OPT_A4                : Option4
    OPT_B5                : Option5
    OPT_LETTER            : Option6
    OPT_POSTCARD          : Option7

}




*Command: CmdCopies
{
    *Order: DOC_SETUP.60
    *Cmd: "<1B>%<1" %2d[1,99]{NumOfCopies }
}


