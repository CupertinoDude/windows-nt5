    *FeatureType:  PRINTER_PROPERTY
    *rcNameID: 363
    *%Installable?: TRUE
    *DefaultOption: NotInstalled
    *Option: NotInstalled
    {
	*rcNameID:  444
	*Constraints:  LIST(OutputBin.Option3)
    }
    *Option: Installed
    {
	*rcNameID:  443
    }
}

*% Collate (Repro function if a Printer Hard Disk is installed).
*Feature: Collate
{
    *rcNameID: 392 
    *DefaultOption: OFF
    
    *Option: OFF
    {
	*rcNameID: 2091
	*Command: CmdSelect
	{
	    *Order: DOC_SETUP.4
	    *Cmd: "@PJL SET QTY=1<0D0A>"
	}
    }
    *Option: ON
    {
	*rcNameID: 2090
	*switch: PrinterHardDisk
	{
	    *case: NotInstalled  *% Can't do Repro copies, set Repro qty to 1.
	    {
		*Command: CmdSelect
		{
		    *Order: DOC_SETUP.4
		    *Cmd: "@PJL SET QTY=1<0D0A>"
		}
	    }
	    *case: Installed    *% Can do Repro copies, set Repro qty count. 
	    {
		*Command: CmdSelect
		{
		    *Order: DOC_SETUP.4
		    *Cmd: "@PJL SET QTY=" %d[1,999]{NumOfCopies}"<0D0A>"
		}
	    }
	}
    }
}

*Feature: PrinterHardDisk
{
    *rcNameID: 430    
    *%Installable?: TRUE
    *FeatureType: PRINTER_PROPERTY
    *DefaultOption: NotInstalled
    *Option: NotInstalled
    {
	*rcNameID: 444
	*DisabledFeatures: LIST(Collate)
    }
    *Option: Installed
    {
	*rcNameID: 443
    }
}

*Command: CmdStartDoc
{
    *Order: DOC_SETUP.10
    *Cmd: "<1B>&u600D<1B>*r0F"
}
*Command: CmdStartPage
{
    *Order: PAGE_SETUP.1
    *Cmd: "<1B>*b0M<0D>"
}
*Command: CmdEndJob
{
    *Order: JOB_FINISH.1
    *Cmd: "<1B>E<1B>%%-12345X"
}

*% Set number of copies if Collate (Repro) is off, else set to 1.
*switch: Collate
{
    *case: OFF
    {
	*Command: CmdCopies
	{
	    *Order: DOC_SETUP.41
	    *% Set uncollated copies count.
	    *Cmd: "<1B>&l" %d[1,999]{NumOfCopies}"X"
	}
    }
    *case: ON
    {
	*Command: CmdCopies
	{
	    *Order: DOC_SETUP.41
	    *% Printer collate, so set copy count to 1.
	    *Cmd: "<1B>&l1X"
	}
    }
}

