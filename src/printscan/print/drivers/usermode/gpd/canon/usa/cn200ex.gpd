*Feature: Resolution
{
    *rcNameID: =RESOLUTION_DISPLAY
    *DefaultOption: Option1
    *Option: Option1
    {
        *Name: "360 x 360 " =DOTS_PER_INCH
        *DPI: PAIR(360, 360)
        *TextDPI: PAIR(360, 360)
        *PinsPerLogPass: 1
        *PinsPerPhysPass: 1
        *MinStripBlankPixels: 0
        EXTERN_GLOBAL: *SendMultipleRows?: FALSE
        EXTERN_GLOBAL: *StripBlanks: LIST(TRAILING)
        *SpotDiameter: 100
        *Command: CmdSendBlockData { *Cmd : "<1B>(A" %l{NumOfDataBytes + 1}"K" }
        *Command: CmdSelect
        {
            *Order: DOC_SETUP.3
            *Cmd: "<1B>(d<020001>h"
        }
    }
    *Option: Option2
    {
        *Name: "180 x 180 " =DOTS_PER_INCH
        *DPI: PAIR(180, 180)
        *TextDPI: PAIR(180, 180)
        *PinsPerLogPass: 1
        *PinsPerPhysPass: 1
        *MinStripBlankPixels: 0
        EXTERN_GLOBAL: *SendMultipleRows?: FALSE
        EXTERN_GLOBAL: *StripBlanks: LIST(TRAILING)
        *SpotDiameter: 100
        *Command: CmdSendBlockData { *Cmd : "<1B>(A" %l{NumOfDataBytes + 1}"K" }
        *Command: CmdSelect
        {
            *Order: DOC_SETUP.3
            *Cmd: "<1B>(d<020000B4>"
        }
    }
}
*Feature: MediaType
{
    *rcNameID: =MEDIA_TYPE_DISPLAY
    *DefaultOption: Option1
    *Option: Option1
    {
        *rcNameID: 262
        *switch: TextQuality
        {
         *case: Option1 {*Command: CmdSelect { *Order: DOC_SETUP.5
                                               *Cmd: "<1B>(c<02001100>"} }          *% Normal
         *case: Option2 {*Command: CmdSelect { *Order: DOC_SETUP.5
                                               *Cmd: "<1B>(c<02001101>"} }          *% High Quality
         *case: Option3 {*Command: CmdSelect { *Order: DOC_SETUP.5
                                               *Cmd: "<1B>(c<02001102>"} }          *% Draft Quality
        }
    }
    *Option: TRANSPARENCY
    {
        *Name: "Transparency"
        *switch: TextQuality
        {
         *case: Option1 {*Command: CmdSelect { *Order: DOC_SETUP.5
                                               *Cmd: "<1B>(c<02001120>"} }       *% Normal
         *case: Option2 {*Command: CmdSelect { *Order: DOC_SETUP.5
                                               *Cmd: "<1B>(c<02001121>"} }       *% High Quality
         *case: Option3 {*Command: CmdSelect { *Order: DOC_SETUP.5
                                               *Cmd: "<1B>(c<02001122>"} }       *% Draft Quality
        }
    }
    *Option: Option3
    {
        *rcNameID: 265
        *switch: TextQuality
        {
         *case: Option1 {*Command: CmdSelect { *Order: DOC_SETUP.5
                                               *Cmd: "<1B>(c<02001100>"} }       *% Normal
         *case: Option2 {*Command: CmdSelect { *Order: DOC_SETUP.5
                                               *Cmd: "<1B>(c<02001101>"} }       *% High Quality
         *case: Option3 {*Command: CmdSelect { *Order: DOC_SETUP.5
                                               *Cmd: "<1B>(c<02001102>"} }       *% Draft Quality
        }
    }
}
*Feature: TextQuality
{
    *rcNameID: =TEXT_QUALITY_DISPLAY
    *DefaultOption: Option1
    *Option: Option1
    {
        *rcNameID: 262
    }
    *Option: Option2
    {
        *rcNameID: 276
    }
    *Option: Option3
    {
        *rcNameID: 277
    }
}
*Feature: Halftone
{
    *rcNameID: =HALFTONING_DISPLAY
    *DefaultOption: HT_PATSIZE_AUTO
    *Option: HT_PATSIZE_AUTO
    {
        *rcNameID: =HT_AUTO_SELECT_DISPLAY
    }
*Ifdef: WINNT_50
    *Option: HT_PATSIZE_SUPERCELL_M
    {
        *rcNameID: =HT_SUPERCELL_DISPLAY
    }
*Endif:
    *Option: HT_PATSIZE_6x6_M
    {
        *rcNameID: =HT_DITHER6X6_DISPLAY
    }
    *Option: HT_PATSIZE_8x8_M
    {
        *rcNameID: =HT_DITHER8X8_DISPLAY
    }
}
*Command: CmdStartDoc
{
    *Order: DOC_SETUP.1
    *Cmd: "<1B>[K<0200000F1B>(a<010001>"
}

*Command: CmdEndJob
{
    *Order: JOB_FINISH.1
    *Cmd: "<1B>(b<0100001B>(a<0100001B>(b<0100001B>@"
}
*RotateCoordinate?: FALSE
*RotateRaster?: FALSE
*RotateFont?: FALSE
*TextCaps: LIST(TC_RA_ABLE)
*CursorXAfterCR: AT_PRINTABLE_X_ORIGIN
*BadCursorMoveInGrxMode: LIST(X_PORTRAIT,X_LANDSCAPE)
*YMoveThreshold: *
*switch: Resolution
{
    *case: Option1
    {
        *YMoveUnit: 360
        *Command: CmdYMoveRelDown
        {
            *Cmd:  "<1B>(e<0200>" %m{DestYRel / 2 }
        }
    }
    *case: Option2
    {
        *YMoveUnit: 180
        *Command: CmdYMoveRelDown
        {
            *Cmd:  "<1B>(e<0200>" %m{DestYRel / 4 }
        }
    }
}
*Command: CmdCR { *Cmd : "<0D>" }
*Command: CmdLF { *Cmd : "<0A>" }
*Command: CmdFF { *Cmd : "<0C>" }
*EjectPageWithFF?: TRUE
*Command: CmdEnableTIFF4 { *Cmd : "<1B>(b<010001>" }
*Command: CmdDisableCompression { *Cmd : "<1B>(b<010000>" }
*OutputDataFormat: H_BYTE
*OptimizeLeftBound?: FALSE
*CursorXAfterSendBlockData: AT_GRXDATA_ORIGIN
*CursorYAfterSendBlockData: NO_MOVE

