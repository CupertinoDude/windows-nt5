*Feature: Orientation
{
    *rcNameID: =ORIENTATION_DISPLAY
    *DefaultOption: PORTRAIT
    *Option: PORTRAIT
    {
        *rcNameID: =PORTRAIT_DISPLAY
    }
    *Option: LANDSCAPE_CC270
    {
        *rcNameID: =LANDSCAPE_DISPLAY
    }
}
*Feature: InputBin
{
    *rcNameID: =PAPER_SOURCE_DISPLAY
    *DefaultOption: AUTO
    *Option: MANUAL
    {
        *% Last byte of Input Bin selection command
        *% depends on MediaType selected.

        *rcNameID: =MANUAL_FEED_DISPLAY
        *switch: MediaType
        {
            *case: Option1 
            {
                *Command: CmdSelect 
                {
                    *Order: DOC_SETUP.4
                    *Cmd: "<1B>(l<02001100>"
                } 
            }	 *% Normal
            *case: TRANSPARENCY 
            {
                *Command: CmdSelect 
                {
                    *Order: DOC_SETUP.4
                    *Cmd: "<1B>(l<02001120>"
                } 
            }	 *% Transparency
            *case: Option3 
            {
                *Command: CmdSelect 
                {
                    *Order: DOC_SETUP.4
                    *Cmd: "<1B>(l<02001180>"
                } 
            }	 *% Envelope    
        }
    }
    *Option: AUTO
    {
        *rcNameID: =AUTO_DISPLAY
        *switch: MediaType
        {
            *case: Option1 
            {
                *Command: CmdSelect 
                {
                    *Order: DOC_SETUP.4
                    *Cmd: "<1B>(l<02001400>"
                } 
            }	 *% Normal
            *case: TRANSPARENCY 
            {
                *Command: CmdSelect 
                {
                    *Order: DOC_SETUP.4
                    *Cmd: "<1B>(l<02001420>"
                } 
            }	 *% Transparency
            *case: Option3 
            {
                *Command: CmdSelect 
                {
                    *Order: DOC_SETUP.4
                    *Cmd: "<1B>(l<02001480>"
                } 
            }	 *% Envelope    
        }
    }
}
