*GPDSpecVersion: "1.0"
*CodePage: 1252
*ModelName: "NEC PC-PR750/150R"
*MasterUnits: PAIR(480, 840)
*ResourceDLL: "nc75jres.dll"
*PrinterType: SERIAL
*FontCartSlots: 1
*PrintRateUnit: LPM
*PrintRate: 180

*GPDFileName: "NC7515RJ.GPD"
*Include: "STDNAMES.GPD"
*GPDFileVersion: "1.000"

*HelpFile: "nc75jres.hlp"
*% *Macros: HelpIDs
*% {
*% PRINT QUALITIES */
*%    POP_75_PQUALITIESNEW         :7560

*% KANJI CODE */
*%    POP_75_KANJICODE             :7570

*% }

*%======== Orientation ========
*Feature: Orientation
{
    *rcNameID: =ORIENTATION_DISPLAY
    *DefaultOption: PORTRAIT
    *Option: PORTRAIT
    {
        *rcNameID: =PORTRAIT_DISPLAY
    }
    *Option: LANDSCAPE_CC270
    {
        *rcNameID: =LANDSCAPE_DISPLAY
    }
}

*%======== InputBin ========
*Feature: InputBin
{
    *rcNameID: =PAPER_SOURCE_DISPLAY
    *DefaultOption: TracterFeeder
    *Option: TracterFeeder
    {
        *rcNameID: 258
    }
}

*%======== Resolution ========
*Feature: Resolution
{
    *rcNameID: =RESOLUTION_DISPLAY
    *DefaultOption: 168dpi
    *Option: 168dpi
    {
        *Name: "160 x 168 dots per inch"
        *DPI: PAIR(160, 168)
        *TextDPI: PAIR(160, 168)
        *PinsPerLogPass: 24
        *PinsPerPhysPass: 24
        *MinStripBlankPixels: 120
        EXTERN_GLOBAL: *StripBlanks: LIST(LEADING,ENCLOSED,TRAILING)
        *SpotDiameter: 120
        *Command: CmdSendBlockData { *Cmd : "<1B>H<1B>e11<1B>J" %4d{NumOfDataBytes / 3} }
    }
}

*%======== PaperSize ========
*Feature: PaperSize
{
    *rcNameID: =PAPER_SIZE_DISPLAY
    *DefaultOption: 15X11
    *Option: 10X11
    {
        *rcNameID: =10X11_DISPLAY
        *PrintableArea: PAIR(4320, 8830)
        *PrintableOrigin: PAIR(240, 205)
        *Command: CmdSelect
        {
            *Order: DOC_SETUP.4
            *Cmd: "<1B>H<1B>L000<1C>05v1320"
        }
    }
    *Option: 15X11
    {
        *rcNameID: =15X11_DISPLAY
        *PrintableArea: PAIR(6528, 8830)
        *PrintableOrigin: PAIR(336, 205)
        *Command: CmdSelect
        {
            *Order: DOC_SETUP.4
            *Cmd: "<1B>H<1B>L000<1C>05v1320"
        }
    }
    *Option: CUSTOMSIZE
    {
        *rcNameID: =USER_DEFINED_SIZE_DISPLAY
        *MinSize: PAIR(960, 840)
        *MaxSize: PAIR(7920, 17909)
        *MaxPrintableWidth: 6528
        *MinLeftMargin: 0
        *CenterPrintable?: FALSE
        *switch: InputBin
        {
            *case: TracterFeeder
            {
                *CustCursorOriginX: %d{0}
                *CustCursorOriginY: %d{0}
                *CustPrintableOriginX: %d{0}
                *CustPrintableOriginY: %d{0}
                *CustPrintableSizeX: %d{min(6528, PhysPaperWidth)}
                *CustPrintableSizeY: %d{PhysPaperLength - (0+0)}
            }
        }
        *Command: CmdSelect
        {
            *Order: DOC_SETUP.4
            *Cmd: "<1B>H<1B>L000<1C>05v" %4d{(PhysPaperLength / 7) }
        }
    }
}

*%======== JIS ON/OFF ========
*Feature: JISONOFF
{
    *rcNameID: 284
    *DefaultOption: JIS90
    *FeatureType: PRINTER_PROPERTY
    *HelpIndex: 7570
    *Option: JIS90
    {
        *rcNameID: 285
        *Command: CmdSelect
        {
            *Order: JOB_SETUP.2
            *Cmd: "<1C>05F2-02"
        }
    }
    *Option: JIS78
    {
        *rcNameID: 286
        *Command: CmdSelect
        {
            *Order: JOB_SETUP.2
            *Cmd: "<1C>05F2-00"
        }
    }
}

*%======== PrintQuality ========
*Feature: PrintQuality
{
    *rcNameID: =TEXT_QUALITY_DISPLAY
    *DefaultOption: Standard
    *HelpIndex: 7560
    *Option: Standard
    {
        *rcNameID: 259
        *Command: CmdSelect
        {
            *Order: DOC_SETUP.2
            *Cmd: "<1B>d1"
        }
    }
    *Option: Draft
    {
        *rcNameID: 261
        *Command: CmdSelect
        {
            *Order: DOC_SETUP.2
            *Cmd: "<1B>d0"
        }
    }
}

*%======== Halftone ========
*Feature: Halftone
{
    *rcNameID: =HALFTONING_DISPLAY
    *DefaultOption: HT_PATSIZE_AUTO
    *Option: HT_PATSIZE_AUTO
    {
        *rcNameID: =HT_AUTO_SELECT_DISPLAY
    }
