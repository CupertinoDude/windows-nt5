           *Order: PAGE_FINISH.1
           *Cmd: "<1B>b"
        }
    }
    *case: Option2
    {
        *EjectPageWithFF?: FALSE
        *Command: CmdEndPage
        {
            *Order: PAGE_FINISH.1
            *Cmd: "<1B>b"
        }
    }
    *case: Option3
    {
        *EjectPageWithFF?: FALSE
        *Command: CmdEndPage
        {
            *Order: PAGE_FINISH.1
            *Cmd: "<1B>b"
        }
    }
    *case: Option4
    {
        *EjectPageWithFF?: TRUE
    }
    *case: Option5
    {
        *EjectPageWithFF?: TRUE
    }
}

