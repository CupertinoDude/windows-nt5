*%======== InputBin ========
*Feature: InputBin
{
    *rcNameID: =PAPER_SOURCE_DISPLAY
    *DefaultOption: SheetGuide
    *Option: SheetGuide
    {
        *rcNameID: 305
        *Constraints: PaperSize.10X11
    }
    *Option: SheetFeeder
    {
        *rcNameID: 306
        *Installable?: TRUE
        *rcInstallableFeatureNameID: 306
        *Constraints: PaperSize.A5_ROTATED
        *Constraints: PaperSize.JAPANESE_POSTCARD
        *Constraints: PaperSize.10X11
        *Command: CmdSelect
        {
            *Order: DOC_SETUP.5
            *Cmd: "<1B>m1"
        }
    }
    *Option: TractorFeeder
    {
        *rcNameID: 310
        *Installable?: TRUE
        *rcInstallableFeatureNameID: 310
        *Constraints: PaperSize.A4
        *Constraints: PaperSize.A5
        *Constraints: PaperSize.A5_ROTATED
        *Constraints: PaperSize.B5
        *Constraints: PaperSize.JAPANESE_POSTCARD
    }
    *Option: HagakiFeeder
    {
        *rcNameID: 314
        *Installable?: TRUE
        *rcInstallableFeatureNameID: 314
        *Constraints: PaperSize.A4
        *Constraints: PaperSize.A5
        *Constraints: PaperSize.A5_ROTATED
        *Constraints: PaperSize.B5
        *Constraints: PaperSize.10X11
        *Constraints: PaperSize.CUSTOMSIZE
        *Command: CmdSelect
        {
            *Order: DOC_SETUP.5
            *Cmd: "<1B>m1"
        }
    }
}

*InvalidInstallableCombination: LIST(InputBin.SheetFeeder, InputBin.TractorFeeder)
