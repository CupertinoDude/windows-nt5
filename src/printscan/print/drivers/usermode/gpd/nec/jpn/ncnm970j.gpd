        {
            *Order: DOC_SETUP.4
            *Cmd: "<1A>G<0D1B>C0"
        }
    }
}

*%======== PaperSize ========
*Feature: PaperSize
{
    *rcNameID: =PAPER_SIZE_DISPLAY
    *DefaultOption: A4
    *Option: A4
    {
        *rcNameID: =A4_DISPLAY
        *PrintableArea: PAIR(2832, 3938)
        *PrintableOrigin: PAIR(72, 94)
        *Command: CmdSelect
        {
            *Order: DOC_SETUP.3
            *Cmd: "<1B>H<1B>L000" %v{(PhysPaperLength / 60) + 15}
        }
    }
    *Option: A5
    {
        *rcNameID: =A5_DISPLAY
        *PrintableArea: PAIR(1954, 2706)
        *PrintableOrigin: PAIR(72, 94)
        *switch: KyuusiIti
        {
            *case: Center
            {
                *Command: CmdSelect
                {
                    *Order: DOC_SETUP.3
                    *Cmd: "<1B>H<1B>L012" %v{(PhysPaperLength / 60) + 15}
                }
            }
            *case: Left
            {
                *Command: CmdSelect
                {
                    *Order: DOC_SETUP.3
                    *Cmd: "<1B>H<1B>L000" %v{(PhysPaperLength / 60) + 15}
                }
            }
        }
    }
    *Option: A5_ROTATED
    {
        *rcNameID: =A5_ROTATED_DISPLAY
        *PrintableArea: PAIR(2832, 1828)
        *PrintableOrigin: PAIR(72, 94)
        *Command: CmdSelect
        {
            *Order: DOC_SETUP.3
            *Cmd: "<1B>H<1B>L000" %v{(PhysPaperLength / 60) + 15}
        }
    }
    *Option: B5
    {
        *rcNameID: =B5_DISPLAY
        *PrintableArea: PAIR(2436, 3372)
        *PrintableOrigin: PAIR(72, 94)
        *switch: KyuusiIti
        {
            *case: Center
            {
                *Command: CmdSelect
                {
                    *Order: DOC_SETUP.3
                    *Cmd: "<1B>H<1B>L006" %v{(PhysPaperLength / 60) + 15}
                }
            }
            *case: Left
            {
                *Command: CmdSelect
                {
                    *Order: DOC_SETUP.3
                    *Cmd: "<1B>H<1B>L000" %v{(PhysPaperLength / 60) + 15}
                }
            }
        }
    }
    *Option: JAPANESE_POSTCARD
    {
        *rcNameID: =JAPANESE_POSTCARD_DISPLAY
        *PrintableArea: PAIR(1274, 1828)
        *PrintableOrigin: PAIR(72, 94)
        *switch: KyuusiIti
        {
            *case: Center
            {
                *Command: CmdSelect
                {
                    *Order: DOC_SETUP.3
                    *Cmd: "<1B>H<1B>L022" %v{(PhysPaperLength / 60) + 15}
                }
            }
            *case: Left
            {
                *Command: CmdSelect
                {
                    *Order: DOC_SETUP.3
                    *Cmd: "<1B>H<1B>L000" %v{(PhysPaperLength / 60) + 15}
                }
            }
        }
    }
    *Option: 10X11
    {
        *rcNameID: =10X11_DISPLAY
        *PrintableArea: PAIR(2880, 3744)
        *PrintableOrigin: PAIR(360, 94)
        *Command: CmdSelect
        {
            *Order: DOC_SETUP.3
            *Cmd: "<1B>H<1B>L000" %v{(PhysPaperLength / 60) }
        }
    }
    *Option: CUSTOMSIZE
    {
        *rcNameID: =USER_DEFINED_SIZE_DISPLAY
        *MinSize: PAIR(720, 360)               *% 2' x 1'
        *MaxSize: PAIR(3600, 7668)             *% (10', 21.3')
        *MaxPrintableWidth: 2880               *% 80 keta
        *MinLeftMargin: 0
        *CenterPrintable?: FALSE
        *switch: InputBin
        {
            *case: SheetGuide
            {
                *CustCursorOriginX: %d{0}
                *CustCursorOriginY: %d{94}
                *CustPrintableOriginX: %d{0}
                *CustPrintableOriginY: %d{94}
                *CustPrintableSizeX: %d{min(2880, PhysPaperWidth)}
                *CustPrintableSizeY: %d{PhysPaperLength - (94+176)}
            }
            *case: SheetFeeder
            {
                *CustCursorOriginX: %d{0}
                *CustCursorOriginY: %d{94}
                *CustPrintableOriginX: %d{0}
                *CustPrintableOriginY: %d{94}
                *CustPrintableSizeX: %d{min(2880, PhysPaperWidth)}
                *CustPrintableSizeY: %d{PhysPaperLength - (94+176)}
            }
            *case: TractorFeeder
            {
                *CustCursorOriginX: %d{0}
                *CustCursorOriginY: %d{0}
                *CustPrintableOriginX: %d{0}
                *CustPrintableOriginY: %d{0}
                *CustPrintableSizeX: %d{min(2880, PhysPaperWidth)}
                *CustPrintableSizeY: %d{PhysPaperLength - (0+0)}
            }
            *case: HagakiFeeder
            {
                *CustCursorOriginX: %d{0}
                *CustCursorOriginY: %d{94}
                *CustPrintableOriginX: %d{0}
                *CustPrintableOriginY: %d{94}
                *CustPrintableSizeX: %d{min(2880, PhysPaperWidth)}
                *CustPrintableSizeY: %d{PhysPaperLength - (94+176)}
            }
        }
        *Command: CmdSelect
        {
            *Order: DOC_SETUP.3
            *Cmd: "<1B>H<1B>L000" %v{(PhysPaperLength / 60) }
        }
    }
}


