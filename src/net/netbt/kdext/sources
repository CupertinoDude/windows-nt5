    -DRASAUTODIAL -D_PNP_POWER_=1 -DBACK_FILL=1 \
    -D_IO_DELETE_DEVICE_SUPPORTED -DMULTIPLE_WINS

DLLENTRY=DllInit    
UMTYPE=windows
UMRES=$(O)\netbtkd.res
USE_NTDLL=1

INCLUDES=..\inc;$(BASE_INC_PATH);$(DDK_INC_PATH)

TARGETLIBS=\
    $(SDK_LIB_PATH)\kernel32.lib \
    $(SDK_LIB_PATH)\ntdll.lib \
    $(SDK_LIB_PATH)\wsock32.lib

SOURCES=\
    netbtkd.c \
    netbtkd.rc

