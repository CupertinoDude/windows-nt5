CM_ROOT=$(PROJECT_ROOT)\rras\cm
!include $(CM_ROOT)\sources.inc

TARGETNAME=cmproxy
TARGETPATH=$(CM_BUILD_ROOT)
TARGETTYPE=DYNLINK

DLLDEF=cmproxy.def
