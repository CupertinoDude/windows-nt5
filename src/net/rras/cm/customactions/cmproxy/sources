USE_NTDLL=1

TARGETLIBS=\
    $(CM_BUILD_ROOT)\*\cmdebug.lib \
    $(CM_BUILD_ROOT)\*\cmutil.lib \
    $(SDK_LIB_PATH)\advapi32.lib \
    $(SDK_LIB_PATH)\kernel32.lib \
    $(SDK_LIB_PATH)\user32.lib \

SOURCES=\
    main.cpp   \
    cmproxy.cpp  \
    util.cpp	\
    cmproxy.rc \
