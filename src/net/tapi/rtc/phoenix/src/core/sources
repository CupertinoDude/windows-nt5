TARGETNAME=RTCCore
TARGETPATH=obj
TARGETTYPE=LIBRARY

!if "$(BUILD_PRODUCT)" != "NT"
! if !$(FREEBUILD)
DEBUG_CRTS=1
C_DEFINES=$(C_DEFINES) -D_DEBUG
! endif
!endif

C_DEFINES=$(C_DEFINES) -DUNICODE -D_UNICODE 

!if !$(FREEBUILD)
C_DEFINES=$(C_DEFINES) -DDUMP_PRESENCE -DRTCLOG
!endif


PRECOMPILED_CXX=1
PRECOMPILED_INCLUDE=stdafx.h
USE_ATL=1
ATL_VER=30
USE_MSVCRT=1
USE_NATIVE_EH=1

INCLUDES=\
      ..\inc; \
      ..\inc\$(O)

