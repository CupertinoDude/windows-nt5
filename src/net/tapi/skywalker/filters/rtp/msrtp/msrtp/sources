RTPROOT=$(MAKEDIR:\msrtp\msrtp=)
!include $(RTPROOT)\common.inc

#MSC_WARNING_LEVEL=/W3 /WX
#NOT_LEAN_AND_MEAN=1
UMTYPE=windows

TARGETNAME=msrtp
TARGETPATH=$(RTPBIN)
TARGETTYPE=DYNLINK

SOURCES= \
    msrtp.rc \
    msrtpapi.c \
    msrtp.c

INCLUDES=\
    $(INCLUDES)

LINKLIBS=\
    $(LINKLIBS) \
    $(RTPBIN)\*\funcs.lib \
    $(RTPBIN)\*\misc.lib \
    $(RTPBIN)\*\rtp.lib \
    $(RTPBIN)\*\rtcp.lib \
    $(RTPBIN)\*\glob.lib

TARGETLIBS=\
    $(TARGETLIBS) \
    $(SDK_LIB_PATH)\kernel32.lib \
    $(SDK_LIB_PATH)\ws2_32.lib \
    $(SDK_LIB_PATH)\advapi32.lib \
    $(SDK_LIB_PATH)\winmm.lib \
    $(SDK_LIB_PATH)\rtutils.lib

DLLDEF=msrtp.def
