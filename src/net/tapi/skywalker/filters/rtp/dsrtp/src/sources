RTPROOT=$(MAKEDIR:\dsrtp\src=)

!include $(RTPROOT)\common.inc

MAJORCOMP=TAPI
MINORCOMP=dsrtp

#MSC_WARNING_LEVEL=/W3 /WX
#NOT_LEAN_AND_MEAN=1
UMTYPE=windows

TARGETNAME=$(MINORCOMP)
TARGETPATH=$(RTPBIN)
!if $(DXMRTP)
TARGETTYPE=LIBRARY
!else
TARGETTYPE=DYNLINK
!endif

BROWSER_INFO=1
C_DEFINES=\
    $(C_DEFINES) \
    -D_DSRTP_

SOURCES= \
    dsrtp.rc \
    dsrtp.cpp \
    source.cpp \
    render.cpp \
    rtpcom.cpp \
    setmtype.cpp

INCLUDES=\
    $(INCLUDES)

!if !$(DXMRTP)
LINKLIBS=\
    $(LINKLIBS) \
    $(RTPBIN)\*\dsrtpapi.lib \
    $(RTPBIN)\*\funcs.lib \
    $(RTPBIN)\*\misc.lib \
    $(RTPBIN)\*\rtp.lib \
    $(RTPBIN)\*\rtcp.lib \
    $(RTPBIN)\*\glob.lib

!if $(FREEBUILD)
LINKLIBS=\
    $(LINKLIBS) \
    $(SDK_LIB_PATH)\strmbase.lib
!else
LINKLIBS=\
    $(LINKLIBS) \
    $(SDK_LIB_PATH)\strmbasd.lib
!endif

TARGETLIBS=\
    $(TARGETLIBS) \
    $(SDK_LIB_PATH)\kernel32.lib \
    $(SDK_LIB_PATH)\ws2_32.lib \
    $(SDK_LIB_PATH)\advapi32.lib \
    $(SDK_LIB_PATH)\ole32.lib \
    $(SDK_LIB_PATH)\user32.lib \
    $(SDK_LIB_PATH)\winmm.lib \
    $(SDK_LIB_PATH)\uuid.lib \
    $(SDK_LIB_PATH)\rtutils.lib

DLLENTRY=_DllMainCRTStartup

DLLDEF=dsrtp.def

COFFBASE=$(TARGETNAME)

!endif
