TARGETNAME=com
TARGETPATH=obj
TARGETTYPE=LIBRARY

!IFNDEF MCAST
MCAST=1
!ENDIF

!IFNDEF SPX
SPX=0
!ENDIF

!IFNDEF SUPPORT612WINS
SUPPORT612WINS=1
!ENDIF

!IFNDEF REG_N_QUERY_SEP
REG_N_QUERY_SEP=1
!ENDIF

!IFNDEF INIT_TIME_PAUSE_TEST
INIT_TIME_PAUSE_TEST=1
!ENDIF

!IFNDEF QFE
QFE=0
!ENDIF

!IFNDEF NEWNETBTIF
NEWNETBTIF=1
!ENDIF

!IFNDEF TEST_HEAP
TEST_HEAP=0
!ENDIF

!IFNDEF PRSCONN
PRSCONN=1
!ENDIF


USENETBT=1
UNICODE=1
NET_C_DEFINES=-DUNICODE

COMMON_DEFINES=-DINCL32= -DNT -DWIN32 -DRPC_NO_WINDOWS_H -DTEST_DATA -DUSENETBT=$(USENETBT) -DQFE=$(QFE) -DSUPPORT612WINS=$(SUPPORT612WINS) -DNEWNETBTIF=$(NEWNETBTIF) -DREG_N_QUERY_SEP=$(REG_N_QUERY_SEP) -DINIT_TIME_PAUSE_TEST=$(INIT_TIME_PAUSE_TEST) -DTEST_HEAP=$(TEST_HEAP) -DMCAST=$(MCAST) -DSPX=$(SPX) -D_PNP_POWER_ -DPRSCONN=$(PRSCONN)

!IFDEF WINS_INTERACTIVE
C_DEFINES=$(COMMON_DEFINES) -DWINS_INTERACTIVE
!ELSE
C_DEFINES=$(COMMON_DEFINES)
!ENDIF

INCLUDES=..\..\msgs\$(O);..\inc;$(DS_INC_PATH)

