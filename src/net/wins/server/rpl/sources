TARGETNAME=rpl
TARGETPATH=obj
TARGETTYPE=LIBRARY

UNICODE=1
NET_C_DEFINES=-DUNICODE

!IFNDEF USENETBT
USENETBT=1
!ENDIF

!IFNDEF SUPPORT612WINS
SUPPORT612WINS=1
!ENDIF

!IFNDEF REG_N_QUERY_SEP
REG_N_QUERY_SEP=1
!ENDIF

!IFNDEF INIT_TIME_PAUSE_TEST
INIT_TIME_PAUSE_TEST=1
!ENDIF

!IFNDEF MCAST 
MCAST=1
!ENDIF

!IFNDEF PRSCONN 
PRSCONN=1
!ENDIF

!IFNDEF  NO_LIMIT 
NO_LIMIT=1
!ENDIF

COMMON_DEFINES=-DINCL_32= -DNT -DWIN32  -DRPC_NO_WINDOWS_H -DTEST_DATA -DUSENETBT=$(USENETBT) -DSUPPORT612WINS=$(SUPPORT612WINS) -DREG_N_QUERY_SEP=$(REG_N_QUERY_SEP) -DINIT_TIME_PAUSE_TEST=$(INIT_TIME_PAUSE_TEST) -DMCAST=$(MCAST) -DNO_LIMIT=$(NO_LIMIT) -DPRSCONN=$(PRSCONN)

!IFDEF WINS_INTERACTIVE
C_DEFINES=$(COMMON_DEFINES) -DWINS_INTERACTIVE
!ELSE
C_DEFINES=$(COMMON_DEFINES)
!ENDIF


INCLUDES=..\..\msgs\$(O);..\inc;$(DS_INC_PATH)

SOURCES=\
    rpl.c \
    rplmsgf.c \
    rplpull.c \
    rplpush.c
