MAJORCOMP=ntos
MINORCOMP=dd

TARGETNAME=msircomm
TARGETPATH=obj
TARGETTYPE=DRIVER

C_DEFINES=-DUNICODE

INCLUDES=..\inc;$(BASE_INC_PATH)
SOURCES=ircomm.rc  \
        initunlo.c \
        pnp.c      \
        util.c     \
        openclos.c \
        write.c    \
        queue.c    \
        read.c     \
        ioctl.c    \
        mask.c

TARGETLIBS=\
    $(DDK_LIB_PATH)\tdi.lib \
    ..\link\$(O)\irlink.lib   \
    ..\util\$(O)\irutils.lib   \
    ..\vuart\$(O)\vuart.lib



PRECOMPILED_INCLUDE=internal.h
PRECOMPILED_PCH=internal.pch
PRECOMPILED_OBJ=internal.obj
