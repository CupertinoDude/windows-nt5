        msg.mc \
        export.cxx \
        import.cxx \
        utils.cxx \
        main.cxx \
        version.rc

TARGETLIBS = \
     ..\dsldap\$(O)\dsldap.lib \
     ..\ldifldap\$(O)\ldifldap.lib \
     ..\utils\$(O)\ldifutils.lib \
        $(SDK_LIB_PATH)\wldap32.lib \
        $(SDK_LIB_PATH)\urlmon.lib \
        $(SDK_LIB_PATH)\imagehlp.lib \
        $(SDK_LIB_PATH)\user32.lib    \
        $(SDK_LIB_PATH)\netapi32.lib    \
        $(SDK_LIB_PATH)\ntdll.lib 
        
     
MC_FLAGS=-v
UMTYPE=console
UMENTRY=wmain

