MAJORCOMP=
MINORCOMP=

TARGETNAME=testldap
TARGETPATH=obj
TARGETTYPE=UMAPPL_NOLIB

INCLUDES=..\..\..\inc;

SUBSYSTEM_VERSION=4.00
SOURCES=testldap.c

USE_MSVCRT=1

UMTYPE=console
UMAPPL=testldap

UMLIBS= $(SDK_LIB_PATH)\ntdll.lib           \
        $(SDK_LIB_PATH)\wldap32.lib  \
        $(SDK_LIB_PATH)\user32.lib

NTDEBUGTYPE=both
NTDEBUG=ntsd
