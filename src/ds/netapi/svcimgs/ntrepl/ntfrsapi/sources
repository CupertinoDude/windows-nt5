USE_MSVCRT=1

TARGETNAME=ntfrsapi
TARGETPATH=obj
TARGETTYPE=DYNLINK

DLLDEF=ntfrsapi.def
DLLENTRY=NtFrsApi_Initialize

INCLUDES=..\inc;..\idl\$(O); \
         $(NET_INC_PATH); \
         $(PROJECT_ROOT)\netapi\inc; \
         $(DS_INC_PATH)\crypto;

TARGETLIBS=\
        $(SDK_LIB_PATH)\rpcrt4.lib \
        $(SDK_LIB_PATH)\kernel32.lib \
        $(SDK_LIB_PATH)\advapi32.lib \
        $(SDK_LIB_PATH)\netapi32.lib \
	$(SDK_LIB_PATH)\user32.lib   \
        $(SDK_LIB_PATH)\wldap32.lib  \
        ..\support\$(O)\frssup.lib

C_DEFINES=$(C_DEFINES) -DUNICODE=1 -D_UNICODE=1 -DLDAP_UNICODE=1 -DDBG=1
!IF "$(NTFRS_TEST)" == "1"
C_DEFINES=$(C_DEFINES) -DNTFRS_TEST=1
!ENDIF 0

SOURCES=\
        ntfrsapi.c \
        frsapi_c_stub.c \
        version.rc

PASS1_PUBLISH= \
   {$(O)\ntfrsapi.lib=$(SDK_LIB_PATH)\ntfrsapi.lib}
