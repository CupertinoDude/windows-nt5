MAJORCOMP=dns
MINORCOMP=dnsexts

TARGETNAME=dnsexts
TARGETPATH=obj
TARGETTYPE=DYNLINK

INCLUDES=..\pinc;..\server\inc;..\server\server;..\dnslib;

DLLDEF=$(O)\dnsexts.def

DLLENTRY=_DllMainCRTStartup

TARGETLIBS= \
        $(SDK_LIB_PATH)\advapi32.lib \
        $(SDK_LIB_PATH)\kernel32.lib \
        $(SDK_LIB_PATH)\ntdll.lib

SOURCES= \
        version.rc  \
        dnsexts.cxx \
        dump.cxx \
        util.cxx


C_DEFINES=$(C_DEFINES)
