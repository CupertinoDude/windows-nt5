MAJORCOMP=drt
MINORCOMP=drtutils

USE_MSVCRT=1
TARGETNAME=drtutils
TARGETPATH=obj
TARGETTYPE=LIBRARY

INCLUDES=.;..\..\include

SOURCES= \
         sconv.cxx

CDEFINES= -DUNICODE -D_UNICODE

UMTYPE=console
UMTEST=

LINKLIBS=

TARGETLIBS = \
             $(SDK_LIB_PATH)\adsiid.lib \
             $(SDK_LIB_PATH)\noutils.lib  \
             $(SDK_LIB_PATH)\nocairo.lib

UMTYPE=console
UMENTRY=main
