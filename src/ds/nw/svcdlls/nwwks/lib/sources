MAJORCOMP=nw
MINORCOMP=nwwlib

TARGETNAME=nwwlib
TARGETPATH=obj
TARGETTYPE=LIBRARY

INCLUDES=..\inc;$(PROJECT_ROOT)\nw\inc;

SOURCES= \
        lsa.c \
        reg.c \
        misc.c \
        splutil.c

UNICODE=1

NET_C_DEFINES=-DRPC_NO_WINDOWS_H -DNOT_USED

UMTYPE=console

UMLIBS= \
    $(O)\nwwlib.lib

OPTIONAL_UMTEST=
