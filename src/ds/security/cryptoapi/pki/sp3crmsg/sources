MAJORCOMP=crypt
MINORCOMP=sp3crmsg

TARGETNAME=$(MINORCOMP)
TARGETPATH=obj
TARGETTYPE=DYNLINK

DLLDEF=$(O)\$(MINORCOMP).def
DLLENTRY=DllMain

C_DEFINES= -D_WINDOWS

NOT_LEAN_AND_MEAN=1
USE_MSVCRT=1
NO_NTDLL=1
UMTYPE=windows

!IF "$(CRYPT_USE_DEBUG_CRTS)"!=""
DEBUG_CRTS=1
!ENDIF

INCLUDES= $(SECURITY_INC);

SOURCES= \
    $(MINORCOMP).rc \
    $(MINORCOMP).cpp

TARGETLIBS= \
    $(SDK_LIB_PATH)\kernel32.lib \
    $(SDK_LIB_PATH)\version.lib \
    $(SDK_LIB_PATH)\crypt32.lib \
    $(SDK_LIB_PATH)\advapi32.lib
