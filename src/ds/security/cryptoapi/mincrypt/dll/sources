!ENDIF

MAJORCOMP=mincrypt
MINORCOMP=mincryptdll

TARGETNAME=$(MINORCOMP)
TARGETPATH=obj
TARGETTYPE=DYNLINK

DLLDEF=$(O)\$(MINORCOMP).def
DLLENTRY=_DllMainCRTStartup

C_DEFINES= -D_WINDOWS

!IF !$(FREEBUILD)
C_DEFINES=$(C_DEFINES) -D_ADVAPI32_
!ENDIF


SYNCHRONIZE_DRAIN=1
NOT_LEAN_AND_MEAN=1
USE_MSVCRT=1
UMTYPE=windows

!IF "$(CRYPT_USE_DEBUG_CRTS)"!=""
DEBUG_CRTS=1
!ENDIF

!IF "$(CRYPT_USE_WARNING_LEVEL_W4)"!=""
MSC_WARNING_LEVEL=/W4
!ENDIF

INCLUDES= $(SECURITY_INC);$(ENDUSER_INC_PATH);

LINKER_FLAGS=/VERBOSE /MAP $(LINKER_FLAGS)

SOURCES= \
    $(MINORCOMP).rc \
    $(MINORCOMP).cpp

LINKLIBS = \
    $(SECURITY_LIB_PATH)\mincrypt.lib


TARGETLIBS= \
    $(SDK_LIB_PATH)\kernel32.lib \
    $(DS_LIB_PATH)\rsa32.lib \
    $(DS_LIB_PATH)\mscat32.lib
