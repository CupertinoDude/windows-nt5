MAJORCOMP=crypt
MINORCOMP=tcrmsg

TARGETNAME=$(MINORCOMP)
TARGETPATH=obj
TARGETTYPE=PROGRAM

C_DEFINES= -D_WINDOWS

UMTYPE=console
NOT_LEAN_AND_MEAN=1

USE_MSVCRT=1


!IF "$(CRYPT_USE_DEBUG_CRTS)"!=""
DEBUG_CRTS=1
!ENDIF

INCLUDES= $(SECURITY_INC);

PRECOMPILED_INCLUDE = global.hxx
PRECOMPILED_PCH =     global.pch
PRECOMPILED_OBJ =     global.obj

SOURCES= \
    $(MINORCOMP).rc \
    $(MINORCOMP).cpp

LINKLIBS = \
    $(SECURITY_LIB_PATH)\pkiutil.lib

TARGETLIBS= \
    $(SDK_LIB_PATH)\crypt32.lib
