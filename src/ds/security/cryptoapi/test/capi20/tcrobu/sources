MAJORCOMP=crypt
MINORCOMP=tcrobu

TARGETNAME=$(MINORCOMP)
TARGETPATH=obj
TARGETTYPE=PROGRAM

C_DEFINES= -D_WINDOWS

UMTYPE=console
NOT_LEAN_AND_MEAN=1

USE_MSVCRT=1


!IF "$(CRYPT_USE_DEBUG_CRTS)"!=""
DEBUG_CRTS=1
!ENDIF

INCLUDES= $(SECURITY_INC);..\certtest; \
    $(DS_INC_PATH)\crypto;

SOURCES= \
    $(MINORCOMP).rc \
    $(MINORCOMP).cpp

LINKLIBS = \
    $(SECURITY_LIB_PATH)\certtest.lib

TARGETLIBS= \
    $(SDK_LIB_PATH)\crypt32.lib \
    $(SDK_LIB_PATH)\cryptnet.lib \
    $(DS_LIB_PATH)\rsa32.lib
