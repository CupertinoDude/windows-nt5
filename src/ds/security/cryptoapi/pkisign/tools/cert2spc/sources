MAJORCOMP=crypt
MINORCOMP=Cert2Spc

TARGETNAME=$(MINORCOMP)
TARGETPATH=obj
TARGETTYPE=PROGRAM

C_DEFINES= -D_WINDOWS

UMTYPE=console
UMENTRY=wmain

NOT_LEAN_AND_MEAN=1

USE_MSVCRT=1


!IF "$(CRYPT_USE_DEBUG_CRTS)"!=""
DEBUG_CRTS=1
!ENDIF

INCLUDES=$(SECURITY_INC);..\toolutl;

SOURCES= \
    $(MINORCOMP).rc \
    $(MINORCOMP).cpp


LINKLIBS = \
    $(SECURITY_LIB_PATH)\unicode.lib \
    $(SDK_LIB_PATH)\user32.lib 	 \
    $(SECURITY_LIB_PATH)\toolutl.lib

TARGETLIBS= \
    $(SDK_LIB_PATH)\crypt32.lib

