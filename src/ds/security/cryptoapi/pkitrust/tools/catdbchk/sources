    $(SDK_LIB_PATH)\crypt32.lib  \
    $(SDK_LIB_PATH)\wintrust.lib  \
    $(SDK_LIB_PATH)\user32.lib \
    $(SDK_LIB_PATH)\ole32.lib \
    $(SDK_LIB_PATH)\msvcrt.lib \
