MAJORCOMP=crypt
MINORCOMP=dumpcat

TARGETNAME=$(MINORCOMP)
TARGETPATH=obj
TARGETTYPE=PROGRAM

INCLUDES= ..\..\common; \
          $(SECURITY_INC);

C_DEFINES= -D_WINDOWS

UMTYPE=console
UMENTRY=wmain

#SYNCHRONIZE_DRAIN=1
USE_MFC=1
USE_MSVCRT=1


!IF "$(CRYPT_USE_DEBUG_CRTS)"!=""
DEBUG_CRTS=1
!ENDIF

SOURCES= \
    $(MINORCOMP).rc \
    $(MINORCOMP).cpp

TARGETLIBS= \
    $(SDK_LIB_PATH)\user32.lib \
    $(SDK_LIB_PATH)\wintrust.lib \
    $(SECURITY_LIB_PATH)\unicode.lib \
    $(SDK_LIB_PATH)\crypt32.lib \
    $(SECURITY_LIB_PATH)\authclib.lib
