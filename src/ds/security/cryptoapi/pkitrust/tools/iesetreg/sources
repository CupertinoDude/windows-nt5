MAJORCOMP=crypt
MINORCOMP=iesetreg

TARGETNAME=$(MINORCOMP)
TARGETPATH=obj
TARGETTYPE=PROGRAM

C_DEFINES= -D_WINDOWS

NOT_LEAN_AND_MEAN=1	

UMTYPE=windows
UMENTRY=wwinmain

USE_MSVCRT=1


!IF "$(CRYPT_USE_DEBUG_CRTS)"!=""
DEBUG_CRTS=1
!ENDIF

INCLUDES= $(SECURITY_INC);

SOURCES= \
    $(MINORCOMP).rc	\
    $(MINORCOMP).cpp

TARGETLIBS= \
	$(SECURITY_LIB_PATH)\unicode.lib \
	$(SDK_LIB_PATH)\user32.lib    \
    $(SDK_LIB_PATH)\advapi32.lib

