MAJORCOMP=crypt
MINORCOMP=osacheck

TARGETNAME=$(MINORCOMP)
TARGETPATH=obj
TARGETTYPE=PROGRAM

INCLUDES= $(SECURITY_INC);$(DS_INC_PATH)

C_DEFINES=$(C_DEFINES) -D_WINDOWS

UMTYPE=console
UMENTRY=main

ATL_VER=30
USE_STATIC_ATL=1

SOURCES= \
    $(MINORCOMP).cpp

USE_MSVCRT=1

!IF "$(CRYPT_USE_DEBUG_CRTS)"!=""
DEBUG_CRTS=1
!ENDIF

TARGETLIBS= \
    $(SDK_LIB_PATH)\crypt32.lib \
    $(SDK_LIB_PATH)\cryptui.lib
