MAJORCOMP=crypt
MINORCOMP=initpki

TARGETNAME=$(MINORCOMP)
TARGETPATH=obj
TARGETTYPE=DYNLINK

DLLDEF=$(O)\$(MINORCOMP).def
DLLENTRY=_DllMainCRTStartup

C_DEFINES= -D_WINDOWS

UMTYPE=console
UMENTRY=wmain

PRECOMPILED_INCLUDE = global.hxx
PRECOMPILED_PCH =     global.pch
PRECOMPILED_OBJ =     global.obj

INCLUDES= $(DS_INC_PATH); \
          $(SECURITY_INC); \
          $(ENDUSER_INC_PATH); \
          ..\common;

SOURCES= \
    $(MINORCOMP).rc     \
    $(MINORCOMP).cpp    \
    initacl.cpp         \
    mvcerts.cpp         \
    pkireg.cpp          \
    tcrack.cpp

NOT_LEAN_AND_MEAN=1
USE_MSVCRT=1

!IF "$(CRYPT_USE_DEBUG_CRTS)"!=""
DEBUG_CRTS=1
!ENDIF

LINKLIBS = \
    $(SECURITY_LIB_PATH)\unicode.lib       \
    $(SECURITY_LIB_PATH)\pkiutil.lib       \
    $(DS_LIB_PATH)\crypt32p.lib 	\
    $(SDK_LIB_PATH)\user32.lib          \
    $(SDK_LIB_PATH)\kernel32.lib        \
    $(SDK_LIB_PATH)\advapi32.lib

TARGETLIBS= \
    $(SDK_LIB_PATH)\wintrust.lib

