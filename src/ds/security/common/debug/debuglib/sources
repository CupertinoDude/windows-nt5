MINORCOMP=dsysdbg

TARGETNAME=dsysdbg
TARGETPATH=$(SECURITY_LIB_DEST)
TARGETTYPE=LIBRARY

INCLUDES=.;..\inc;..\..\inc;..\..\..\inc;

USE_MSVCRT=1

SOURCES=        debug.c \
                assert.c \
                except.c \
                safelock.c

