MAJORCOMP=net
MINORCOMP=hello2s

TARGETNAME=hello2s
TARGETPATH=obj
TARGETTYPE=LIBRARY


UMTYPE=console
UMAPPL=hello2s
UMLIBS= \
        $(O)\hello2s.lib \
        $(SDK_LIB_PATH)\rpcrt4.lib \


UNICODE=1

INCLUDES=..;

SOURCES= \
        hello2_s.c

