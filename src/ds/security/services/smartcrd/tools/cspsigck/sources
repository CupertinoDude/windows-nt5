    $(SECURITY_LIB_PATH)\CalaisLb.lib \
    $(CRTLIBS) \
    $(LIBC_LIB)

# Use C++ Exception Handling
USE_NATIVE_EH=1

# Use MFC, without or with UNICODE
USE_MFC=1
USE_MSVCRT=1
USE_IOSTREAM=1

# Make it a console application
UMTYPE=console
UMENTRY=main
