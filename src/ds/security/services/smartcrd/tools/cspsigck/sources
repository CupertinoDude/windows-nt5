MAJORCOMP=Calais
MINORCOMP=cspsigck

TARGETNAME=cspsigck
TARGETPATH=$(SECURITY_LIB_DEST)
TARGETTYPE=PROGRAM

SOURCES= \
    cspsigck.cpp \
    cspsigck.rc

