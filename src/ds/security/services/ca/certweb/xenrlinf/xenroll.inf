[version]
signature="$CHICAGO$"
AdvancedINF=2.0

[Add.Code]
xenroll.dll=xenroll.dll

[xenroll.dll]
file-win32-x86=x86/xenroll.dll
file-win32-ia64=ia64/xenroll.dll

;force file download
FileVersion=-1,-1,-1,-1

;DestDir is downloaded program files
DestDir= 

;force download to DestDir even if control is already registered somewhere else
ForceDestDir=1 

