MAJORCOMP=userenv
MINORCOMP=profmap

TARGETNAME=profmap
TARGETTYPE=DYNLINK
TARGETPATH=$(SECURITY_LIB_DEST)

# compiler options
USE_MSVCRT=1
C_DEFINES=-DUNICODE

# include path
INCLUDES=..\rpc;..\rpc\$(O);\
          $(WINDOWS_INC_PATH);\
          $(COM_INC_PATH);

NTTARGETFILE0= $(O)\pmapapi_s.c \
               $(O)\pmapapi_c.c

SOURCES=profmap.c   \
        debug.c     \
        userenv.c   \
        $(O)\pmapapi_s.c \
        $(O)\pmapapi_c.c \
        poolmem.c   \
        reg.c       \
        regrep.c    \
        profmap.rc


# linker options
DLLDEF=profmap.def
DLLENTRY=_DllMainCRTStartup

# link libraries
TARGETLIBS= \
        $(SDK_LIB_PATH)\kernel32.lib        \
        $(SDK_LIB_PATH)\ntdll.lib           \
        $(SDK_LIB_PATH)\advapi32.lib        \
        $(SDK_LIB_PATH)\user32.lib          \
        $(SDK_LIB_PATH)\rpcutil.lib         \
        $(SDK_LIB_PATH)\rpcndr.lib          \
        $(SDK_LIB_PATH)\rpcrt4.lib          \
        $(SDK_LIB_PATH)\netapi32.lib        \
        $(SDK_LIB_PATH)\uuid.lib            \
        $(SDK_LIB_PATH)\userenv.lib
