TARGETNAME=testgina
TARGETPATH=obj
TARGETTYPE=LIBRARY

INCLUDES=.

SOURCES= \
        testgina.c \
        desktops.c \
        debug.c \
        winutil.c \
        validate.c \
        windproc.c \
        wlx.c \
        res.rc

UMAPPL=testgina
UMTYPE=windows
UMENTRY=winmain

UMLIBS=                                             \
       $(O)\testgina.lib \
       $(O)\res.res \
       $(SDK_LIB_PATH)\kernel32.lib         \
       $(SDK_LIB_PATH)\advapi32.lib         \
       $(SDK_LIB_PATH)\user32.lib \
       $(SDK_LIB_PATH)\ntdll.lib \
       $(SDK_LIB_PATH)\gdi32.lib \
       $(SDK_LIB_PATH)\shell32.lib \
       $(SDK_LIB_PATH)\comdlg32.lib \
       $(SDK_LIB_PATH)\comctl32.lib

USE_MSVCRT=1
