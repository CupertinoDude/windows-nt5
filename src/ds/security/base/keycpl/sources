MAJORCOMP=
MINORCOMP=

NEPTUNE_ROOT=..
SETUP_ROOT=$(NEPTUNE_ROOT)\..

TARGETNAME=keymgr
TARGETPATH=obj
TARGETTYPE=DYNLINK
TARGETEXT=cpl

C_DEFINES=$(C_DEFINES) -DUNICODE -D_UNICODE
USE_CRTDLL = 1

INCLUDES= \
    $(SECURITY_INC)\$(O);

MISCFILES= Key_Manager_CPL_{20B49A2C-5030-4F1F-BF71-F7372548E4E1}.sld

SOURCES=                           \
        keymgr.cpp \
        keymgr.rc

TARGETLIBS = \
        $(O)\keymgr.lib               \
        $(SDK_LIB_PATH)\comctl32.lib   \
        $(SDK_LIB_PATH)\kernel32.lib   \

