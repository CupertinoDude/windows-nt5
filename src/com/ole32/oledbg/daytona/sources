TARGETNAME=oledbg
TARGETPATH=obj
TARGETTYPE=DYNLINK


INCLUDES=..;..\..\..\h

DLLDEF=oledbg.def
DLLENTRY=_DllMainCRTStartup
!IF $(IA64)
DLLBASE=0x100110000
!ELSE
DLLBASE=0x1110000
!ENDIF

SOURCES= \
         ..\oledbg.cxx



TARGETLIBS=                                             \
       $(SDK_LIB_PATH)\kernel32.lib         \
       $(SDK_LIB_PATH)\advapi32.lib         \
       $(SDK_LIB_PATH)\user32.lib           \
       $(SDK_LIB_PATH)\rpcrt4.lib

C_DEFINES=$(C_DEFINES) -DRPC_NO_WINDOWS_H

USE_MSVCRT=1


