MAJORCOMP=    comtools
MINORCOMP=    datagen

!include $(CTCOMTOOLS)\commac.mk

#
# This is the name of the target built from the source files specified
# below.  The name should include neither the path nor the file extension.
#

TARGETNAME=   dg

TARGETPATH=   ..\..\lib\mac

#
# This specifies the type of the target, such as PROGRAM, DYNLINK, LIBRARY,
# etc.
#

TARGETTYPE=   LIBRARY

INCLUDES=     \
              ..\.;                 \
              ..\..\h;              \
              $(CTOLERPC)\include

SOURCES=      \
              ..\dg_base.cxx \
              ..\dg_int.cxx  \
              ..\dg_real.cxx \
              ..\dg_ascii.cxx \
              ..\dg_bstr.cxx

UMTYPE=       macintosh




PRECOMPILED_CXX=     1
PRECOMPILED_INCLUDE= ..\..\h\comtpch.hxx
PRECOMPILED_TARGET=  ..\..\h\mac\$(O)\comtpch.pch
PRECOMPILED_OBJ=     ..\..\h\mac\$(O)\comtpch.obj
PRECOMPILED_OPTION=  /Yucomtpch.hxx /Fp..\..\h\mac\$(O)\comtpch.pch

