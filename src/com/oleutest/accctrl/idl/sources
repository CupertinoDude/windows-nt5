MAJORCOMP   =  oleutest
MINORCOMP   =  accctrl

TARGETNAME=   oactest
TARGETPATH=   obj
TARGETTYPE=  DYNLINK
DLLENTRY=DllMain
DLLDEF=oactest.src
DLLBASE=0x45000000
C_DEFINES= -DREGISTER_PROXY_DLL

INCLUDES=$(INCLUDES);..\h

SOURCES= oactest_p.c \
         oactest_i.c \
         dlldata.c

TARGETLIBS= $(SDK_LIB_PATH)\kernel32.lib \
            $(SDK_LIB_PATH)\rpcrt4.lib \
	    $(SDK_LIB_PATH)\uuid.lib

# Generate stubs from idl files
NTTARGETFILE0=allps
