[Game0]
; 00: Disable
; 01: Enable
HKR,,"Enable Game Port",01,00
HKR,,"Enable MPU-401",01,00

[Game1]
HKR,,"Enable Game Port",01,01
HKR,,"Enable MPU-401",01,01


