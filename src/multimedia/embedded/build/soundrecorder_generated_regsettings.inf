[DefaultInstall]
AddReg=AddReg


[Version]
Signature = "$Windows NT$"
ClassGUID={00000000-0000-0000-0000-000000000000}

[AddReg]
HKCR,".wav","",0x00000002,"SoundRec"
HKCR,".wav\OpenWithList\sndrec32.exe",,0x00000012
HKCR,"AppID\{00020C01-0000-0000-C000-000000000046}","",0x00000002,"%SOUND_RECORDER%"
HKCR,"CLSID\{0003000D-0000-0000-C000-000000000046}","",0x00000002,"%SOUND%"
HKCR,"CLSID\{0003000D-0000-0000-C000-000000000046}\Insertable","",0x00000002,""
HKCR,"CLSID\{0003000D-0000-0000-C000-000000000046}\Ole1Class","",0x00000002,"SoundRec"
HKCR,"CLSID\{0003000D-0000-0000-C000-000000000046}\ProgID","",0x00000002,"SoundRec"
HKCR,"CLSID\{0003000D-0000-0000-C000-000000000046}\TreatAs","",0x00000002,"{00020C01-0000-0000-C000-000000000046}"
HKCR,"SoundRec","",0x00000000,"%WAVE_SOUND%"
HKCR,"SoundRec\CLSID","",0x00000002,"{00020C01-0000-0000-C000-000000000046}"
HKCR,"SoundRec\DefaultIcon","",0x00000002,"sndrec32.exe,1"
HKCR,"SoundRec\protocol",,0x00000012
HKCR,"SoundRec\protocol\StdExecute",,0x00000012
HKCR,"SoundRec\protocol\StdExecute\server","",0x00000002,"sndrec32.exe"
HKCR,"SoundRec\protocol\StdFileEditing",,0x00000012
HKCR,"SoundRec\protocol\StdFileEditing\server","",0x00000002,"sndrec32.exe"
HKCR,"SoundRec\protocol\StdFileEditing\verb",,0x00000012
HKCR,"SoundRec\protocol\StdFileEditing\verb\0","",0x00000002,"%_PLAY%"
HKCR,"SoundRec\protocol\StdFileEditing\verb\1","",0x00000002,"%_EDIT%"
HKCR,"SoundRec\protocol\StdFileEditing\verb\2","",0x00000002,"%_OPEN%"
HKCR,"SoundRec\shell\record","",0x00000002,"%_RECORD%"
HKCR,"SoundRec\shell\record\command","",0x00000002,"sndrec32.exe ""%L"""

