    CONTROL "Level", TS_FLVLGRP, "button", BS_GROUPBOX | WS_GROUP | WS_CHILD, 120, 95, 104, 64
    CONTROL "Off", TSLOG_FOFF, "button", BS_RADIOBUTTON | WS_TABSTOP | WS_CHILD, 125, 107, 44, 12
    CONTROL "Terse", TSLOG_FTERSE, "button", BS_RADIOBUTTON | WS_TABSTOP | WS_CHILD, 125, 119, 56, 12
    CONTROL "Verbose", TSLOG_FVERBOSE, "button", BS_RADIOBUTTON | WS_TABSTOP | WS_CHILD, 125, 133, 68, 12
    CONTROL "OK", TSLOG_OK, "button", BS_DEFPUSHBUTTON | WS_GROUP | WS_TABSTOP | WS_CHILD, 140, 172, 40, 14
    CONTROL "Cancel", TSLOG_CANCEL, "button", BS_PUSHBUTTON | WS_TABSTOP | WS_CHILD, 188, 172, 40, 14
