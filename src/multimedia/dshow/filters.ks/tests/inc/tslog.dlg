LOGGING DIALOG LOADONCALL MOVEABLE DISCARDABLE 4, 2, 240, 190
FONT 8, "Helv"
CAPTION "Set Logging Configuration"
STYLE WS_CAPTION | WS_VISIBLE | DS_MODALFRAME | WS_POPUP
BEGIN
    CONTROL "Logging Output", TS_LOGOUTGRP, "button", BS_GROUPBOX | WS_GROUP | WS_CHILD, 10, 6, 220, 76
    CONTROL "Window", TSLOG_WINDOW, "button", BS_RADIOBUTTON | WS_TABSTOP | WS_CHILD, 20, 25, 47, 12
    CONTROL "COM1", TSLOG_COM1, "button", BS_RADIOBUTTON | WS_TABSTOP | WS_CHILD, 20, 39, 44, 12
    CONTROL "Level", TS_LOGLVLGRP, "button", BS_GROUPBOX | WS_GROUP | WS_CHILD, 120, 14, 104, 64
    CONTROL "Off", TSLOG_OFF, "button", BS_RADIOBUTTON | WS_TABSTOP | WS_CHILD, 126, 25, 27, 12
    CONTROL "Terse", TSLOG_TERSE, "button", BS_RADIOBUTTON | WS_TABSTOP | WS_CHILD, 126, 39, 56, 12
    CONTROL "Verbose", TSLOG_VERBOSE, "button", BS_RADIOBUTTON | WS_TABSTOP | WS_CHILD, 126, 53, 55, 12
    CONTROL "File Logging", TS_FLOGGRP, "button", BS_GROUPBOX | WS_GROUP | WS_CHILD, 10, 83, 220, 87
    CONTROL "Filename", TS_LOGFPRMPT, "button", BS_GROUPBOX | WS_CHILD, 15, 95, 102, 28
